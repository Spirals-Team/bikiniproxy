if(typeof aFloat!==typeof bFloat){aFloat+='';bFloat+='';}
if(aFloat<bFloat){return-1;}
if(aFloat>bFloat){return 1;}}
return 0;},sortNaturalAsc:function(a,b,col,c){if(a===b){return 0;}
