function extractCommonData(data,options,event){var $this=$(this);var searchResultRows;var clickedRow;var $searchResultRow=$this.closest('#searchResultItems, .search-results, .searchEntry, .contentContainer, #frmSearch, #frmSearchResults');if($searchResultRow.length){searchResultRows=$searchResultRow.find('.articleEntry, .searchResultItem, .searchEntry');clickedRow=$this.closest('.articleEntry, .searchResultItem, .searchEntry');}else{$searchResultRow=$this.closest('.searchResultsListing, .result-list');if($searchResultRow.length){searchResultRows=$searchResultRow.find('li');clickedRow=$this.closest('li');}}
if(searchResultRows&&clickedRow){data.searchPageRank=searchResultRows.index(clickedRow)+1;}
