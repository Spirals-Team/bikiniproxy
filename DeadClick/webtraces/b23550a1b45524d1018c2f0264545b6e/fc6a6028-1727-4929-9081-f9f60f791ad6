{if(document.all&&document.all[aButton])
{var x=document.all[aButton];if(x.length)
x=x[0];x.click();}
else
{var x=aForm.action;if((x.type==null)||(x.type.indexOf("select")==-1))
{x+=((x.indexOf("?")>0)?"&":"?");x+=(aButton+".x=1");aForm.action=x;}
