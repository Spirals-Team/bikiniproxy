function popSfxLink(prefix, id, url, doi) {
    if (doi) {
        url += "&id=doi:"+doi;
    } else {
        doi = "10.1080%2F10720162.2015.1081839";
    }
    var redirect = prefix + encodeURIComponent("?sid=tandf&genre=article"+url);
    var href = "/servlet/linkout?suffix="+id+"&dbid=16384&doi="+doi+"&url=" + encodeURIComponent(redirect);
    return newWindow(href);
}

var sfxRefDisplay = {
64:"[ADS]",
32:"CAS",
8192:"CIS",
256:"CSA",
16:"[Crossref]",
128:"[Web of Science &#0174;]",
4096:"JSTOR",
8:"[PubMed]",
