function get_query(){
    var url = location.href;
    var val ="";
    var qs = url.substring(url.indexOf('?') + 1).split('&');
    for(var i = 0, result = {}; i < qs.length; i++){
        qs[i] = qs[i].split('=');
        val = decodeURIComponent(qs[i][1]);
        result[qs[i][0]] = val.replace(/\+/g," ");
    }
    return result;
};

function fLaunchMap(){
    //Initialize map
    if(null == myMap){
        VMLaunch("ViaMichelin.Api.Map", {
            container : mapContainer,//Map container (DOM element)
            //Initializes map center with data from another service called in the onSuccess callback
            center : ViaMichelin.Api.Constants.Map.DELAY_LOADING
        },{
            onInit: function(serviceMap){
                myMap = serviceMap;
            },
            onSuccess: function(){
                fLaunchPoiSearch();
            }});
    }else{//Clean up

        if(myMap)
            myMap.removeAllLayers();
        else
            document.getElementById("dMapContainer").innerHTML = "";
        document.getElementById("results").innerHTML = "";
        POIs = null;
        fLaunchPoiSearch();

    }
};//fLaunchMap


function fLaunchPoiSearch(){
    //Launch proximity search

    VMLaunch("ViaMichelin.Api.Poi", {
        service: ViaMichelin.Api.Constants.Poi.SERVICE_TYPE.FIND_POI,
        db: dbID,//Database ID
        center: {address:{singleFieldSearch:document.getElementById("adresse").value,
            inputCountry : $_id("countrySelector")}},//Search center
        nb: 5,//Number of returned POIs
        criteria:[{id:1,value:1}],
        dist:20000,
        map: {  container: mapContainer,
            focus: true,
            offset: {x : 10, y: 24},
            iconPath: "/extension/gieviamichelin/design/miomcti_public/images/mapiconpoi{ICON_ID}.png"
          }
    },{
        onSuccess : function (results) {
            POIs = results;
            var out = '';
            var res = document.getElementById("results");
            for(var i = 0; i< results.nbFound; ++i){

                res.appendChild(displayPoi(results.poiList[i].poi,i));
            }
            //$_id("results").innerHTML = out;
        },
        onError : function (error) {
            alert('Merci de vrifier votre saisie');
        }});
   

};//fLaunchPoiSearch

function displayPoi(poi,index,full)
{

    var first = "";
    var ct=index+1;
    var couleur="clair";
    if (index % 2 == 0)
        couleur= "sombre";
    if (index == 1)
        first = "first";
    if (full)
        couleur = "full";
    var text = document.createElement('div');
    var textHtml = "";
    text.setAttribute("class","item "+couleur +" "+first);


    textHtml += "<h2>"+ct +" - "+poi.name+"</h2>" +
    "<p>" +
    poi.location.formattedAddressLine+"<br />" +
    poi.location.postalCode + " " +
    poi.location.city + "<br />";
    if (full)
    {
        mailUrl = mailUrlList[poi.datasheet.iconId];

       // console.log(poi.id);

        var domaine = (poi.id.substring(0, 4) == "CIAT")?"POLICE":"GENDARMERIE";
        

        var ref = {'unit': '','libelle': '', 'email' : ''};
        if (poi.datasheet.descList.length)
        {
            ref.libelle = (poi.datasheet.descList[0])? poi.datasheet.descList[0].value: "";
            ref.email = (poi.datasheet.descList[1])? poi.datasheet.descList[1].value: "";

        }
        ref.unit = poi.datasheet.email;
       // console.log(poi);

        textHtml += "tel : "+ ((poi.datasheet.tel)? poi.datasheet.tel.number:"") + "<br />";
        if (poi.datasheet.url)
        {
            textHtml += "<a href=\"" + poi.datasheet.url + "\" target=\"_blank\"> Plus d'infos sur : " + poi.name + "</a>";
        }
        else
        {
            textHtml +=  "<form class=\"viamichelinmail\" method=\"post\" action=\"" + mailUrl + "\"><button type=\"submit\"  >"+
            "Envoyer un mail  l'unit</button> <input type=\"hidden\" name=\"nom_unite\" value=\"" + poi.name + "\" />"+
            "<input type=\"hidden\" name=\"domaine\" value=\""+domaine+"\" />"+
            "<input type=\"hidden\" name=\"send_key\" value=\""+ $.base64.btoa(JSON.stringify(ref))+"\" /></form>";
        }
            // "mail :  <a href=\"mailto:"+poi.datasheet.email+"\" >Envoyer un mail  l'unit </a></p>" +
        textHtml += showItiForm(poi.location.formattedAddressLine+", " + poi.location.postalCode + " " + poi.location.city);
    }
    else
    {
        textHtml += "</p>";
    }



    textHtml += "<hr class=\"clearfloat\" />";
    text.innerHTML = textHtml;

    if (!full)
    {


        if (!text.addEventListener) {
            text.attachEvent("onclick", function(){
                singlePoint(index);
            });
        }
        else {
            text.addEventListener("click", function(){
                singlePoint(index);
            }, false);
        }
    }
    return text;

}

function showItiForm(destAdresse)
{
    var form = "<form id=\"goto\"  onsubmit=\"fetchIti();return false;\" action=\"\" >" +
        "<fieldset><legend>Calcul d'itinraire</legend><p><label>Dpart</label><input type=\"text\" id=\"from\" name=\"from\" placeholder=\"Dpart ...\" /></p>" +
        "<p><label>Arrive</label><input type=\"text\" id=\"to\" name=\"to\" value=\"" + destAdresse +"\" /></p>" +
        "<p><input id=\"itiGoTo\" type=\"submit\" name=\"itineraire\"  value=\"Itinraire\" /></p></fieldset></form>";



    return form;
}

/* Initialisation de l'autocompletion */
function initAutoComplete(fieldID)
{
    JSELaunch("ViaMichelin.Api.Completion.CompletionRCU", {
            input : $_id(fieldID) ,
            inputCountry : $_id("countrySelector")
        },{
            "onInitError" : function(ref, error){ alert("onInitError ref: "+ ref +", error: "+ error); },
            "onError" : function(error){ alert("onError - error: "+ error); } },
        ViaMichelin.Api.Constants.System.ApiJSEPath);
}

/* Recherche d'itineraire */
function fetchIti()
{

    myMap.removeAllLayers();

    if (document.getElementById('dRoadsheet')) document.getElementById('dRoadsheet').innerHTML = "";
    if (document.getElementById('dMultipleIti')) document.getElementById('dMultipleIti').innerHTML = "";

    VMLaunch("ViaMichelin.Api.Itinerary", {
        steps:[//Array of Geo coodinates
            {address:{singleFieldSearch:document.getElementById("from").value,
                inputCountry : $_id("countrySelector")}},
            {address:{singleFieldSearch:document.getElementById("to").value,
                inputCountry : $_id("countrySelector")}}],
        itit: ViaMichelin.Api.Constants.Itinerary.ITINERARY_TYPE.QUICK,

        //Map to display itinerary trace with automatic redraw to fit iti
        map: {container: mapContainer, focus: true},
        roadsheet: $_id("dRoadsheet"),
        multipleIti: false,
        distUnit: ViaMichelin.Api.Constants.Itinerary.DIST_UNIT.METERS
    },{
        onSuccess : function (result) {


            var dist = result.header.summaries[0].totalDist / 1000;
            var d = new Date(result.header.summaries[0].totalTime);
            var m = Math.round(result.header.summaries[0].totalTime/60);



            $_id("dSummary").innerHTML = "<br/>Distance : " +  dist.toFixed(2)  + ' Km<br/>Temps : ' +m+ " minutes";
        },
        onError : function (error) {
            alert('Merci de vrifier votre saisie');
        }});
};

function singlePoint(id)
{

    var PoiID = POIs.poiList[id].poi.id;
    myMap.removeAllLayers();
    POIs= 0;

    VMLaunch("ViaMichelin.Api.Poi", {
        service: ViaMichelin.Api.Constants.Poi.SERVICE_TYPE.POI,
        db: dbID,//Database ID
        poiid:PoiID,//Search center

        map: {  container: mapContainer,
            focus: true,
            offset: {x : 60, y: 0},
            iconPath: "mapiconpoi{ICON_ID}.png"}
    },{
        onSuccess : function (results) {
            POIs = results;


            var layer = results.getLayer();


           // console.log(layer);
            layer.setBubbleContent("<div class=\"bulle\"><h2>"+results.name+"</h2><p>"+ results.location.formattedAddressLine+"<br />" +
            results.location.postalCode + " " +
            results.location.city + "</p></div>");


            var res = document.getElementById('results');
            res.innerHTML = "";
            res.appendChild(displayPoi(results,0,1));


            initAutoComplete("from");
            /*
             document.getElementById('goto').addEventListener('submit',function(e){
             e.preventDefault();
             itineraire();
             });*/
            //------- Correction ---------

            /*Penser  rajouter un id  : itiGoTo au bouton itinraire */

            /* Cette condition gre le addEventListener qui n'est pas reconnu dans les
             versions infrieures d IE9 */

            if (!document.getElementById('itiGoTo').addEventListener) {
                document.getElementById('itiGoTo').attachEvent("onclick", function (e) {
                    if(e.preventDefault) { e.preventDefault();}; // Condition pour IE8
                    fetchIti();
                    return false;
                });
            }else{
                document.getElementById('itiGoTo').addEventListener("click", function (e) {
                    if(e.preventDefault) { e.preventDefault();}; // Condition pour IE8
                    fetchIti();
                    return false;
                }, false);
            }


            //----------------------------



        },
        onError : function (error) {
            alert('Merci de vrifier votre saisie');
        }});


};

function fLoadMap() {
    VMLaunch("ViaMichelin.Api.Map", {//Service parameters
        //Map container (DOM element)
        center : { coords : {lon: 2.9, lat: 46}},
        container : $_id("dMapContainer"),
        mapTypeControl : true,
        mapTypeControlOptions : {type: ViaMichelin.Api.Constants.Map.TYPE.LIGHT},
        zoom : 6
    }, {
        onSuccess : function (app) {

            var searchConf = {
                "type" : "poi",
                "param" : { 'type':'169046', 'fields':[], 'mode': 'A', 'criteria': [{'type':'RESS_META_NUM_1','operator':'in','value':'[1]'}]},
                "style" : {
                    "grouping" : {
                        "fill" : "blue", //couleur de remplissage du cercle (nom de la couleur ou code couleur html) (type : string)
                        "fill-opacity" : 0.35, //opacit de la couleur de remplissage du cercle (type : float)
                        "stroke" : "blue", //couleur du bord du cercle (nom de la couleur ou code couleur html) (type : string)
                        "stroke-opacity" : 0.35, //opacit de la couleur du bord du cercle (type : float)
                        "stroke-width" : 0, //largeur du bord du cercle (type : int)
                        "shadow-blur" : 0 //discipation de l'ombre en pixel (type : int)
                    },
                    "unitary" : {
                        "fill" : "violet", //couleur de remplissage du cercle (nom de la couleur ou code couleur html) (type : string)
                        "fill-opacity" : 1, //opacit de la couleur de remplissage du cercle (type : float)
                        "stroke" : "#000", //couleur du bord du cercle (nom de la couleur ou code couleur html) (type : string)
                        "stroke-opacity" : 1, //opacit de la couleur du bord du cercle (type : float)
                        "stroke-width" : 1, //largeur du bord du cercle (type : int)
                        "shadow-blur" : 1, //discipation de l'ombre en pixel (type : int)
                        "shadow-offsetX" : 1, //dcalage de l'ombre sur l'axe X (type : int)
                        "shadow-offsetY" : 1, //dcalage de l'ombre sur l'axe Y (type : int)
                        "width" : 4 //largeur du cercle (type : int)
                    }
                }
            };

            var conf = {
                "map" : app,
                "search" : searchConf
            };

            JSELaunch("ViaMichelin.Api.Map.ClusteredPois",
                conf, {
                    "onInit" : function (app) {

                    }
                });
        },
        onInitError : function () {
            alert('La carte n\'a pas pu tre affiche');
        }
    });
};//fLoadMap

function doSearch()
{
    fLaunchMap();
    return false;
};

var myMap = null;
var POIs = null;
var mapContainer = $_id('dMapContainer');

var mcContainer = document.getElementById('dMapContainer');
var mcContainerParent = mcContainer.parentNode;
var mcWidth = mcContainerParent.offsetWidth - 20;
var michelinBlock = document.getElementById('michelinblock');
var w = (mcWidth*3/4) + 3;
w = w + "px";



/* michelinBlock.style.height = */
var h = mcWidth *3/4;
mcContainer.style.height = h+"px";
mcContainer.style.width = mcWidth+"px";

//console.log("mcWidth: "+mcWidth+" h : "+ h);

var q= get_query();
adresse = q.adresse;
country = q.country;

initAutoComplete("adresse");

if (adresse && adresse != "")
{
    document.getElementById('adresse').setAttribute('value',adresse);
    $("#countrySelector").val(country);

    fLaunchMap();
}
else
{
    fLoadMap()
}
