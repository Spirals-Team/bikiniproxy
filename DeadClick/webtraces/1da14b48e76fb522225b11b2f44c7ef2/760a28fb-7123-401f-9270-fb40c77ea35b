		for ( i = 0; i < storytext.childNodes.length; i++) 
		{
			if (storytext.childNodes[i].nodeName.toLowerCase() === 'p' || storytext.childNodes[i].nodeName.toLowerCase() === 'h2') 
			{
				/* Now that we're in a valid element, check to see if we already stored a target position.. if not, this is the position we're targetting */
				if(targetChildElement == null)
				{
					targetChildElement = i;
				}
				
				/* Add the height of the current element to the total height thus far */
				totalHeight += storytext.childNodes[i].clientHeight;
				
				/* 
					If we're skipping a floating element. Then we dont want to count any paragraphs that are wrapping the element as "valid" paragraphs.
					The result could be super squished text.
					Example document: http://money.cnn.com/2015/04/08/news/economy/ben-bernanke-book-federal-reserve/index.html
				*/
				if(heightToSkip > 0)
				{
					heightToSkip -= storytext.childNodes[i].clientHeight;
					resetValues();
				}
				else if(heightToSkip < 0)
				{
					heightToSkip = 0;
				}
				
				/* If we've reached the desired minimum height and we have a target element, then its time to insert */
				if(totalHeight > minHeight && targetChildElement != null)
				{
					//console.log("total height = " + totalHeight);
					//console.log("childNode = " + targetChildElement);
	
					storytext.childNodes[targetChildElement].insertAdjacentHTML('afterend', smartAssetDiv);
