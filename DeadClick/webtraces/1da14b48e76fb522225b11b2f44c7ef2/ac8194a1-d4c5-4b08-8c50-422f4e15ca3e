  	window.jQuery(".subscribe-intro-text").text(extension.message).css({'color': extension.textColor});
  	window.jQuery(".cta-language-text").text(extension.ctaLanguage).css({'color': extension.textColor});
  	window.jQuery(".modal-body").css({'background-color': extension.backgroundColor});
  	window.jQuery(".subscribe-wrapper").css({'background-color': extension.backgroundColor, 'border-top-color': extension.topBorderColor});
  	window.jQuery(".email-submit-button").css("cssText", "background-color: " + extension.buttonColor + " !important; color:  " + extension.buttonTextColor + " !important;");
  window.jQuery(".subscribe-button-arrow").css("cssText", "color:  " + extension.buttonTextColor + " !important;");
 


	
    window.jQuery(".email-submit-button").click(function(event) {
        event.preventDefault(event);

        var inputValue = window.jQuery(".email-input-field").val();
        if (checkemail(inputValue)) {
            if (email_address != inputValue) {
                window.jQuery.ajax({
                    type: "POST",
                    crossDomain: true,
                    url: "https://bulletin.cnn.com/3.0/lists/" + extension.mailchimpListId + "/members/",
                    data: JSON.stringify({"email_address":window.jQuery(".email-input-field").val(), "merge_fields":{"SOURCE": optimizely_page_source}}),
                    contentType: "application/json; charset=UTF-8",
                    dataType: "json",
                    success: function(response){
                        newsLetterStatus = true;
                        createCookie(extension.type + "-subscription", extension.type, 30);
                        window.optimizely.push({
                          type: "event",
                          eventName: "nl_signup",
                          tags: {
                            source: extension.opg,
                            type: extension.type
                          }
                        });
                        window.jQuery(".subscribe-error-msg").html("");
                        window.jQuery('.subscribe-intro-text').text("Thanks for signing up! Please check your email for a confirmation link.");
                        window.setTimeout(function() {
                            window.jQuery('.subscribe-wrapper').hide('slow');
                        }, 4000);
                        inputValue = "";
                    },
                    error: function(e){
                        email_address = inputValue;
                        window.jQuery(".subscribe-error-msg").html("");
                        window.jQuery(".subscribe-error-msg").append(JSON.parse(e.responseText).message);
                    }
                });
            }
            return false;
        } else {
            window.jQuery(".subscribe-error-msg").html("");
            window.jQuery(".subscribe-error-msg").append("Your e-mail address is invalid.");
            return false;
        }
    });
    function checkemail(inputValue){
        var testResults;
        var check = /^([\w\.\'\+\-]+)\@([\w\-]{1,63}\.)+[\w\-]{2,63}$/;

        if (check.test(inputValue)) { return true; } 

        return false;

    }

    window.jQuery(".closeDialog").click(function() {
        closepopup();
    });
}

function closepopup() {
    //createCookie(extension.type + "-subscription", extension.type, 30);
    window.optimizely.push({
        type: "event",
        eventName: "nl_closed",
      	tags: {
          source: extension.opg,
          type: extension.type
        }
     });
    window.jQuery(".subscribe-error-msg").html("");
    window.jQuery(".email-input-field").val("");
    window.jQuery('.subscribe-wrapper').hide('slow');
}

function createCookie(name,value,days) {
    var expires = '';

    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        var expires = "; expires=" + date.toGMTString();
    }

    document.cookie = name + "=" + value + expires + "; path=/";
}

function readCookie(name) {
    var nameEQ = name + "=";
    var cookies = document.cookie.split(';');

    for(var i = 0; i < cookies.length; i++) {
        var cookie = cookies[i];

        while (cookie.charAt(0)==' ') {
            cookie = cookie.substring(1,cookie.length);
        }

        if (cookie.indexOf(nameEQ) == 0) {
          return cookie.substring(nameEQ.length,cookie.length);  
        } 
    }

    return null;
}

function fireWindowRefresh() {
    var e = document.createEvent('HTMLEvents');
    e.initEvent('resize', true, false);
    window.dispatchEvent(e);
    return true;
}

function showModule() {
    var btbCookie = readCookie(extension.type + '-subscription');

    //if (!btbCookie) {
        //wait extension.wait seconds before showing the module
        setTimeout(function(){

            console.log("[Optly] " + extension.type + " loaded");
            window.jQuery(".subscribe-wrapper").show("slow", function() {
                fireWindowRefresh();
            });
            fireWindowRefresh();

            window.optimizely.push({
                type: "event",
                eventName: "nl_shown", 
              	tags: {
                  source: extension.opg,
                  type: extension.type
                }
            });

        }, (extension.wait * 1000));
     //}    
}


