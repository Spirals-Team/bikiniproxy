      function showMyAd() {
        MYTAnimations();
        var counter = 0;
        var adInterval = setInterval(function() {
          resetCSS();
          MYTAnimations();
          counter++;
          if (counter === 1) {
            clearInterval(adInterval);
          }
        }, 15000);
      }
      try {
        var parentState = parent.document.readyState;
        if (parentState === 'complete') {
          document.getElementById('MYT_main_container').style.display = 'block';
          if ((typeof mraid == 'object') && (mraid.getState() == 'loading')) {
            mraid.addEventListener('ready', showMyAd);
          } else {
            showMyAd();
          }
        } else {
          window.addEventListener('load', function() {
            document.getElementById('MYT_main_container').style.display = 'block';
            if ((typeof mraid == 'object') && (mraid.getState() == 'loading')) {
              mraid.addEventListener('ready', showMyAd);
            } else {
              showMyAd();
            }
          });
        }
      } catch (e) {
        document.getElementById('MYT_main_container').style.display = 'block';
        if ((typeof mraid == 'object') && (mraid.getState() == 'loading')) {
          mraid.addEventListener('ready', showMyAd);
        } else {
          showMyAd();
        }
      }
