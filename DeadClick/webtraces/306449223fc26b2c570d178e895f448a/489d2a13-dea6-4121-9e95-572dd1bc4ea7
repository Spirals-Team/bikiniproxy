try {
/* module-key = 'com.adaptavist.confluence.formMailNG:form-discovery', location = 'js/forms/forms-discovery.js' */
AJS.toInit(function () {
    'use strict';

    /**
     * Function to set the local storage
     * @param item
     */
    function setStorageItem(item) {
        if (typeof(Storage) !== "undefined") {
            localStorage.setItem(item, "true");
        }
    }

    /**
     * Get local storage value for a given key
     * @param item
     */
    function getStorageItem(item) {
        if (typeof(Storage) !== "undefined") {
            return localStorage.getItem(item);
        }
    }

    /**
     * Creates a Discovery dialog if it hasn't been acknowledged before
     * @param idToPointTo - id where to point the dialog
     * @param title - the title of the dialog
     * @param body - the body text
     * @param localStorageVariable - what to save
     * @param additionalOptions - for the inline dialog
     */
    Forms.Discovery.create = function (idToPointTo, body, localStorageVariable, additionalOptions, title) {
        var jqueryVar = $('#' + idToPointTo);

        if (jqueryVar.length) {
            var idOfDialog = idToPointTo + '-dialog';
            if (typeof(title) === "undefined") {
                title = "New Feature!";
            }

            var settings = {hideDelay: null, persistent: true};

            if (typeof(additionalOptions) !== "undefined") {
                $.extend(settings, additionalOptions);
            }

            if (getStorageItem(localStorageVariable) === null) {
                var popup = AJS.InlineDialog(
                    jqueryVar,
                    idOfDialog,
                    function (content, trigger, showPopup) {
                        content
                            .html(Forms.Discovery.content({
                                title: title,
                                body: body,
                                idOfDialog: idOfDialog,
                                localStorageVariable: localStorageVariable
                            }));
                        showPopup();
                        return false;
                    },
                    settings
                );
                popup.show();
            }
        }
    };

    Forms.Discovery.onClick = function (element) {
        var idOfDialog = element.getAttribute("idOfDialog");
        var localStorageVariable = element.getAttribute("localStorageVariable");

        setStorageItem(localStorageVariable);

        AJS.$('#inline-dialog-' + idOfDialog).remove();
    };

    if (AJS.params.remoteUser !== '' && AJS.DarkFeatures.getAllEnabled().indexOf("com.adaptavist.confluence.forms.disablepopups") == -1) {
        Forms.Discovery.create('create-page-button', "Discover the new Forms for Confluence Blueprints that can help you set up a form in no time!", "create-page-button");
        Forms.Discovery.create('space-tools-menu-trigger', "Did you know you can now create Form Configurations as a Space Admin?", "space-tools-menu-trigger");
        Forms.Discovery.create('submitButtonId', "We've made Forms for Confluence simpler. Now the submit button, submit image button and the catpcha are all part of the Forms macro itself. Edit the Forms macro and check it out!", "submit-button", {arrowOffsetX: -300});
        Forms.Discovery.create('fm-admin-new-mail-server', "Now you can set a Form Configuration to have a single submission to allow only a single submission to this form (per user). Great for Surveys!", "single-submission");
    }
});
}catch(e){WRMCB(e)};
;
