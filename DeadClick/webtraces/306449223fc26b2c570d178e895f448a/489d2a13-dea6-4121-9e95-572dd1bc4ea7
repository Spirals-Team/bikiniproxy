/* module-key = 'com.onresolve.confluence.groovy.groovyrunner:confluence-feature-discovery', location = 'js/scriptrunner/confluence/feature-discovery.js' */
AJS.toInit(function () {
    'use strict';

    /**
     * Function to set the local storage
     * @param item
     */
    function setStorageItem(item) {
        if (typeof(Storage) !== "undefined") {
            localStorage.setItem(item, "true");
        }
    }

    /**
     * Get local storage value for a given key
     * @param item
     */
    function getStorageItem(item) {
        if (typeof(Storage) !== "undefined") {
            return localStorage.getItem(item);
        }
    }

    /**
     * Creates a Discovery dialog if it hasn't been acknowledged before
     * @param idToPointTo - id where to point the dialog
     * @param title - the title of the dialog
     * @param body - the body text
     * @param localStorageVariable - what to save
     * @param additionalOptions - for the inline dialog
     */
    plugin.com.onresolve.scriptrunner.confluence.create = function (idToPointTo, body, localStorageVariable, additionalOptions, title) {
        var jqueryVar = $('#' + idToPointTo);

        if (jqueryVar.length) {
            var idOfDialog = idToPointTo + '-dialog';
            if (typeof(title) === "undefined") {
                title = "New Feature!";
            }

            var settings = {hideDelay: null, persistent: true};

            if (typeof(additionalOptions) !== "undefined") {
                $.extend(settings, additionalOptions);
            }

            if (getStorageItem(localStorageVariable) === null) {
                var popup = AJS.InlineDialog(
                    jqueryVar,
                    idOfDialog,
                    function (content, trigger, showPopup) {
                        content
                            .html(plugin.com.onresolve.scriptrunner.confluence.content({
                                title: title,
                                body: body,
                                idOfDialog: idOfDialog,
                                localStorageVariable: localStorageVariable
                            }));
                        showPopup();
                        return false;
                    },
                    settings
                );
                popup.show();
            }
        }
    };

    plugin.com.onresolve.scriptrunner.confluence.onClick = function (element) {
        var idOfDialog = element.getAttribute("idOfDialog");
        var localStorageVariable = element.getAttribute("localStorageVariable");

        setStorageItem(localStorageVariable);

        AJS.$('#inline-dialog-' + idOfDialog).remove();
    };

    if (AJS.params.remoteUser !== '' && AJS.DarkFeatures.getAllEnabled().indexOf("com.adaptavist.confluence.forms.disablepopups") == -1 && AJS.params.isSpaceAdmin === true) {
        plugin.com.onresolve.scriptrunner.confluence.create('space-tools-menu-trigger', "Did you know you can configure ScriptRunner Built-in Scripts as a Space Admin?", "space-tools-menu-trigger");
    }
});
}catch(e){WRMCB(e)};
