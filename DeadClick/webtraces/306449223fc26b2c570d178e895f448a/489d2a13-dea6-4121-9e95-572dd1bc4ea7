try {
/* module-key = 'com.onresolve.confluence.groovy.groovyrunner:lazy-loaded-macro-resources', location = 'js/lazy-loaded-macro.js' */
(function ($) {
    $(function () {
        function loadRemote ($el, $contentEl) {

            AJS.$.ajax({
                type: "POST",
                url: AJS.contextPath() + "/rest/scriptrunner-confluence/latest/macros/exec",
                data: {
                    "srJwt": $el.data("jwt")
                },
                beforeSend: function (request) {
                    request.setRequestHeader("X-Atlassian-token", "no-check");
                }
            }).fail(function (XMLHttpRequest, textStatus, errorThrown) {
                $contentEl.empty().append("Failed to execute macro: " + errorThrown);
            }).done(function (data) {
                $contentEl.empty().append(data);
            }).always(function() {
                $(document).trigger("SR_PANEL_REFRESHED");
            })
        }

        function bindMacroTriggers() {
            $(".lazy-loaded-macro-trigger").each(function (n) {
                var $trigger = $(this);
                $trigger.on("click", function (e) {
                    loadRemote($trigger, $trigger.closest("div.sr-lazy-loaded-macro-container"));
                });
            });
        }

        $(document).on("SR_PANEL_REFRESHED", function () {
            bindMacroTriggers();
        });

        $("div.sr-lazy-loaded-macro").each(function (n) {
            var $div = $(this);
            loadRemote($div, $div);

        });

        bindMacroTriggers();
    });
})(AJS.$);
}catch(e){WRMCB(e)};
;
