    loadForgotPasswordColorBox = function(){

        var top = jQuery(window).height() / 2 - 190;

        jQuery.colorbox({
            speed       : 450,
            height      : "370px",
            width       : "480px",
            top         : top + "px",
            maxWidth    : "100%",
            opacity     : 0.5,
            scrolling   : false,
            open 		: true,
            inline		: true,
            closeButton : false,
            className   : "autopopup",
            href		:".checkout-forgotpassword"
        });
    };

