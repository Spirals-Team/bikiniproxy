 * Arrow style changed MVC callback
 */
InfoBubble.prototype.arrowStyle_changed = function() {
    this.arrowSize_changed();
};
InfoBubble.prototype['arrowStyle_changed'] =
    InfoBubble.prototype.arrowStyle_changed;


