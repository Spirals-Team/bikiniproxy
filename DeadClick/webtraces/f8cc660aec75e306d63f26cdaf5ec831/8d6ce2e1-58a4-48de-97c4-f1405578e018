  margin: 0;
  padding: 0;
  direction: ltr; }

/* Default Link Styles */
a {
  color: #e95144 /*#627f9a*/;
  text-decoration: none;
  line-height: inherit; }
  a:hover, a:focus {
    color: #000; }
  a img {
    border: none; }

