<script>
//Mobile Card Selector
$(document).ready(function() {
//Limit Key Press Classes
  $(".input-pos-float").limitkeypress({ rexp: /^[+]?\d*\.?\d*$/ });
  $(".input-pos-int").limitkeypress({ rexp: /^[+]?\d*$/ });
  $(".input-numberonly").limitkeypress({ rexp: /^[0-9.]*$/ });
  $(".input-pos-int").limitkeypress({ rexp: /^[0-9- ]*$/ });
  $(".input-value").limitkeypress({ rexp: /^[0-9]*$/ });
  $(".input-textonly").limitkeypress({ rexp: /^[a-zA-Z- ]*$/ });
  $(".input-address").limitkeypress({ rexp: /^[a-zA-Z0-9- ]*$/ });
  $("#UserEmail").limitkeypress({ rexp: /^[a-zA-Z0-9-_@.]*$/ }); 
});

</script>
  

