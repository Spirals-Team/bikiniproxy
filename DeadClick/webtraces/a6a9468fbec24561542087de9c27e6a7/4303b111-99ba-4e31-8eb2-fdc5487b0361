	<script> 
		function checkRegFields() {
		
			alarm($('#validateReg').val);
			
			if(document.getElementById("validateReg").value != document.getElementById("validateReg2").value) {
				alarm('The email is not the same');
				return false;
			} else {
				alarm('Please, continue');
				return true;
			}
			
			
			
			return false;
		}
	</script>
	
