<script type="text/javascript">

			$(document).ready(function(){
			
			$("a.openMessageButton").click(function(){
				$(this).hide();
				$(".paragraph").slideDown();
				$(".easter-egg").css("margin-bottom", "80px");
				$("a.closeMessageButton").show();
			
			  });
			
			  $("a.closeMessageButton").click(function(){
				$(this).hide();
				$(".paragraph").slideUp();
				$(".easter-egg").css("margin-bottom", "0");
				$("a.openMessageButton").show();
			  });
			  
			  
			});
</script>



