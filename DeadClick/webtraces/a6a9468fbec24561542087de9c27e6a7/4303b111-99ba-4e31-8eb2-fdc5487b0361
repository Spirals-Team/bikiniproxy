$(document).ready(function() {
			if(location.href.search("travel-money")!=-1){ 
			$("#travel-money-tab").addClass("selectedTab");			
			}
			if(location.href.search("prepaid-currency-card")!=-1){ 
			$("#prepaid-currency-card-tab").addClass("selectedTab");			
			}	
			if(location.href.search("click-and-collect")!=-1){ 
			$("#click-and-collect-tab").addClass("selectedTab");			
			}
			if(location.href.search("money-transfer")!=-1){ 
			$("#money-transfer-tab").addClass("selectedTab");			
			}
			if(location.href.search("deals-and-offers")!=-1){ 
			$("#deals-and-offers-tab").addClass("selectedTab");			
			}
			if(location.href.search("my-account")!=-1){ 
			$("#my-account-tab").addClass("selectedTab");			
			}						
			$("#showCopy").click(function() {
			$("#itemCurrencyContent").show("slow");
			});
			
