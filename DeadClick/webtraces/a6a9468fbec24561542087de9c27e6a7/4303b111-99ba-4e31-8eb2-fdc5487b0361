	var currently_showing = '';
	function switch_currency(buysell) {
		var selection = document.getElementById("currency_selector");
		var el = '';
		if (currently_showing != '') {
			el = document.getElementById('div_' + currently_showing);
			el.style.display = 'none'; 
		}

