PMVpaidPlayer.prototype.tpAdSkippedHandler=function(a){this._pmVPAID.executeTrackingEvent(TrackingEventTypes.SKIP);this._pmVPAID.dispatchAdEvent(AdEvent.AD_SKIPPED);this._tpVPAIDAddom.removeEventListener(AdEvent.AD_SKIPPED,this.tpAdSkippedHandler)};PMVpaidPlayer.prototype.tpAdStartedHandler=function(a){Logger.info("Inside PMVpaidPlayer - TPV dispatched Ad_Started event");this._pmVPAID.dispatchAdEvent(AdEvent.AD_STARTED);this._tpVPAIDAddom.removeEventListener(AdEvent.AD_STARTED,this.tpAdStartedHandler)};
PMVpaidPlayer.prototype.tpAdStoppedHandler=function(a){this._protocolTimer.stop();Logger.info("Inside PMVpaidPlayer - TPV dispatched AD_STOPPED event");this.resetPMPlayer();this._pmVPAID._isImpressionEventDispached?this._pmVPAID.dispatchAdEvent(AdEvent.AD_STOPPED):this.handleVpaidError(ErrorMessage.PM_ERR_VPAID_AD_ERROR_DISPATCHED,"DSP stopped ad before ad impression.")};PMVpaidPlayer.prototype.tpAdUserAcceptInvitationHandler=function(a){this._pmVPAID.dispatchAdEvent(AdEvent.AD_USER_ACCEPT_INVITATION)};
PMVpaidPlayer.prototype.tpAdUserCloseHandler=function(a){this._pmVPAID.dispatchAdEvent(AdEvent.AD_USER_CLOSE)};PMVpaidPlayer.prototype.tpAdUserMinimizeHandler=function(a){this._pmVPAID.dispatchAdEvent(AdEvent.AD_USER_MINIMIZE)};
PMVpaidPlayer.prototype.tpAdVideoCompleteHandler=function(a){Logger.info("Inside PMVpaidPlayer - TPV dispatched AD_VIDEO_COMPLETE event");this._pmVPAID.executeTrackingEvent(TrackingEventTypes.COMPLETE);this._pmVPAID.dispatchAdEvent(AdEvent.AD_VIDEO_COMPLETE);this._tpVPAIDAddom.removeEventListener(AdEvent.AD_VIDEO_COMPLETE,this.tpAdVideoCompleteHandler)};
