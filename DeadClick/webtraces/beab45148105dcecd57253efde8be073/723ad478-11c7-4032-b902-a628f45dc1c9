<script>
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";path=/; " + expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length,c.length);
        }
    }
    return "";
}

function updateMobileMenu(){
	jQuery(".pt_product-search-result .filter-container div.breadcrumb").html(jQuery(".pt_product-search-result .search-result-content div.breadcrumb").html());
	jQuery(".pt_product-search-result .mobile-filter-count").html("("+jQuery("#secondary > div.category-container.white-bg.clearfix > div.filter-container > div.filter-container-inner > div.refinementsAttr").find("[class^=selected]").length+")"); 
	//jQuery(".pt_product-search-result .mobile-filter-count").html("("+jQuery(".filter-container .selected").length+")");
	//jQuery(".pt_product-search-result .mobile-filter-count").html("("+jQuery(".selected").length+")"); 
}

function isTouchDevice() {
    return 'ontouchstart' in document.documentElement;
}

function addGridSwipe(){
	jQuery("#search-result-items").off("swiperight").on( "swiperight",function(){
		if (isTouchDevice()) {
			//$('body').prepend('<div id="loading-temp" style="position:fixed;height:1000px;width:100%;background:#fff;opacity:0.8;z-index:10000"><div style="width:100px;margin-left: -50px;margin-right: auto;display: block;float: none;left: 50%;top: 200px;border:none;" class="zoomPreload">&nbsp;</div></div>');
			jQuery("a.page-previous").trigger("click");
		}
	});
	jQuery("#search-result-items").off("swipeleft").on( "swipeleft",function(){
		if (isTouchDevice()) {
			//$('body').prepend('<div id="loading-temp" style="position:fixed;height:1000px;width:100%;background:#fff;opacity:0.8;z-index:10000"><div style="width:100px;margin-left: -50px;margin-right: auto;display: block;float: none;left: 50%;top: 200px;border:none;" class="zoomPreload">&nbsp;</div></div>');
			jQuery("a.page-next").trigger("click");
		}
	});

}
function resizeWindow(){
	jQuery(".refinement h3:not(.projectproducts-title)").off("click").on("click",function(event){
		 event.stopImmediatePropagation();
		 jQuery(this).parent().siblings().find(".arrow-up").hide();
		if(jQuery(this).parent().find(".arrow-up").css("display") == "block"){

			jQuery(this).parent().find(".arrow-up").hide();
		}
		else {
			jQuery(this).parent().find(".arrow-up").show();
		}

		if($(this).parent().hasClass("dropdown-clearance-sale")) {
			$(this).closest(".is-tablet-mobile").siblings().find(".dropopen").removeClass("dropopen");
			$(this).closest(".is-tablet-mobile").siblings().find(".downarrow").removeClass("downarrow");
		}
		
		jQuery(this).parent().siblings().find(".dropopen").removeClass("dropopen");
		jQuery(this).parent().siblings().find(".downarrow").removeClass("downarrow");
		
		

		if(jQuery(this).parent().find("ul.dropopen").length > 0){
			jQuery(this).parent().find(".dropopen").removeClass("dropopen");
			jQuery(this).parent().find(".downarrow").removeClass("downarrow");
		}
		else{
			jQuery(this).find(".dropopen").removeClass("dropopen");
			jQuery(this).find(".downarrow").removeClass("downarrow");

			jQuery(this).parent().find("ul").addClass("dropopen");
			jQuery(this).parent().find("h3").addClass("downarrow");
			jQuery(this).parent().find("h2").addClass("downarrow");
		}
		return false;
	})

	jQuery(".category-refinement h2.refinement-header").eq(0).off().on("click",function(event){
		event.stopImmediatePropagation();
		// event.stopPropagation();

		$(this).find('downarrow').removeClass("dropopen");
		$(this).find('dropopen').removeClass("dropopen");

		jQuery(this).parent().siblings().find(".dropopen").removeClass("dropopen");
		jQuery(this).parent().siblings().find(".downarrow").removeClass("downarrow");
		
		if(jQuery(this).parent().find("ul.dropopen").length > 0){
			jQuery(this).parent().find(".dropopen").removeClass("dropopen");
			jQuery(this).parent().find(".downarrow").removeClass("downarrow");
		}
		else{
			jQuery(this).find(".dropopen").removeClass("dropopen");
			jQuery(this).find(".downarrow").removeClass("downarrow");

			jQuery(this).parent().find("ul#category-level-1").addClass("dropopen");
			jQuery(this).parent().find("h3").addClass("downarrow");
			jQuery(this).parent().find("h2").addClass("downarrow");
		}
		return false;
	})

	$(window).load(function() {
		setScrollConstant()
	});
	loadScrollPage();
	return false;

}
function addLoadingOverlay(){


}

function loadRefinementCount(){
	var refinementFilters=jQuery(".refinement-filters");
	for(x=0;x<refinementFilters.length;x++){
		var refinementFiltersI=jQuery(refinementFilters).eq(x).parent().find("li.selected").length;
		if(refinementFiltersI>0){
			jQuery(refinementFilters).eq(x).parent().find("span.projectproduct_count").html("("+refinementFiltersI+")");
		}
	}


}
function addGridSlot(){
	if(jQuery(".pt_product-search-result .grid-tile.grid-tile-slot").length>0){
		var slotPosition=parseInt(jQuery(".pt_product-search-result .grid-tile.grid-tile-slot").attr("data-slot-position"));
		var slotContent=jQuery(".pt_product-search-result .grid-tile.grid-tile-slot").html();
		if(isNaN(slotPosition)==false&&slotContent.trim()!="" && jQuery(".pt_product-search-result .grid-tile.grid-tile-slot").children().length>0){
			jQuery(".grid-tile.grid-slot").remove();
			jQuery("<li class='grid-tile grid-slot'>"+slotContent+"</li>").insertAfter(".grid-tile:eq("+(slotPosition-1)+")");

		}


	}

}
function setScrollConstant(){
	jQuery(".pt_product-search-result #secondary .white-bg").removeClass("fixed-top");
	jQuery(".mobile-category-header").removeClass("fixed-top");
	jQuery(".search-found-container .mobile-search").eq(0).css("position","static");
	jQuery(".pt_product-search-result .search-result-content div.breadcrumb").eq(0).css("position","static");
	if(jQuery(window).width()<1024){
		scrollvalue=$(".mobile-category-header").offset().top;
	}
	else{
		scrollvalue=$('.white-bg').offset().top;
	}
	setResultOptoionHeader();
	loadScrollPage();
}

function setResultOptoionHeader() {
	if(jQuery(window).width()<1024){
		jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("position","static").css("top","auto");
	} else {
		//jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("position","absolute").css("top","-141px");
	}
}

function loadScrollPage(){
	 var scroll = jQuery(window).scrollTop();

		if(jQuery(window).width()<1024){
		    	jQuery(".pt_product-search-result .search-result-found .primary-content").css("width",jQuery(window).width());
		    	if(jQuery(".sort-by-dom-label.show-mobile-sort").hasClass("downarrow")){
		    		jQuery("ul.mobile-sort").attr('style',"display:none !important");
		    		jQuery(".sort-by-dom-label.show-mobile-sort").removeClass("downarrow");
		   	    }
		}
		else{
			jQuery(".pt_product-search-result .search-result-found .primary-content").css("width","100%");

		}

		if(jQuery(window).width()>0){
		    if(scroll>=scrollvalue && scroll!=0){
		    	//Only Do Sticky Filters On Mobile
		    	if(jQuery(window).width()<1024) {
			    	jQuery(".pt_product-search-result #secondary .white-bg").addClass("fixed-top");
			    	jQuery(".mobile-category-header").addClass("fixed-top");
			    	jQuery(".search-found-container .mobile-search").eq(0).css("position","fixed");
			    	jQuery(".pt_product-search-result .search-result-content div.breadcrumb").eq(0).css("position","fixed");
			    	jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("position","fixed");

			    	if(jQuery(window).width()>1024){

			    		jQuery(".pt_product-search-result #secondary .white-bg.fixed-top").css("top",jQuery(".search-found-container .mobile-search").eq(0).height());
						jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("top",jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result #secondary .white-bg").height());

			    	}
			    	else	{
			    		jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("top",jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result .mobile-category-header").height());
			    		jQuery(".pt_product-search-result .search-result-content div.breadcrumb").eq(0).css("top",jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).height()+jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result .mobile-category-header.fixed-top").height());
			    		jQuery(".pt_product-search-result #secondary .white-bg.fixed-top").css("top",0);

			    	}

			    	$(".hr-divider").remove();
					$("body").append("<hr class='hr-divider' style='background-color: #ccc;border-bottom:1px solid #ccc;border-top:6px solid #fff;width:100%;position:fixed;top:"+(jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result #secondary .white-bg").height()+jQuery(".search-result-mobile-header.search-result-options-top.scrolldown").height()-8)+"px;left:0;z-index:996'/>");
					$(".pt_product-search-result #secondary .search-result-mobile-header").addClass("scrolldown");
				}
		    }
		    else	{
		    	if(jQuery(".sort-by-dom-label.show-mobile-sort").hasClass("downarrow")){
		    		jQuery("ul.mobile-sort").attr('style',"display:none !important");
		    		jQuery(".sort-by-dom-label.show-mobile-sort").removeClass("downarrow");
			    }
				jQuery(".pt_product-search-result #secondary .white-bg").removeClass("fixed-top");
		    	jQuery(".mobile-category-header").removeClass("fixed-top");
		    	jQuery(".search-found-container .mobile-search").eq(0).css("position","static");
		    	jQuery(".pt_product-search-result .search-result-content div.breadcrumb").eq(0).css("position","static");

		    	setResultOptoionHeader();

		    	$(".hr-divider").remove();
				$(".pt_product-search-result #secondary .search-result-mobile-header").removeClass("scrolldown");

		    }
	    }
		else{
	    	jQuery(".pt_product-search-result #secondary .white-bg").removeClass("fixed-top");
	    	jQuery(".mobile-category-header").removeClass("fixed-top");
	    	jQuery(".search-found-container .mobile-search").eq(0).css("position","static");
	    	jQuery(".pt_product-search-result .search-result-content div.breadcrumb").eq(0).css("position","static");
	    	jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).css("position","absolute");

	    	setResultOptoionHeader();

		}

}

var doit;
var scrollvalue=0;

jQuery(document).ready(function(){
	jQuery(".quickview_options").on("click",function(){
		jQuery(this).parent().parent().find(".product-image .thumb-link").trigger('hover');
		jQuery(this).parent().parent().find(".product-image .thumb-link").trigger('mouseover');
		jQuery(this).parent().parent().find(".product-image .thumb-link").trigger('mouseenter');
		jQuery("#quickviewbutton").trigger("click");
	});


	jQuery(".pt_product-search-result div.breadcrumb").css('max-width',jQuery(window).width());




	//jQuery(".white-bg .refinement .toggle span").off("click").on("click",function(){
		//jQuery(this).parent().parent().trigger("click");

	//})




	//jQuery(".refinement ul").hide(); - Why would someone do this????

	jQuery("button.filter-reset").on("click",function(){
		setCookie("grid-dropdown-active-class","");
		$(".dropopen").removeClass("dropopen");
		$(".downarrow").removeClass("downarrow");
		window.location.href=$(this).find("a").attr("href");
	});

	if(jQuery(window).width()<1024){
    	jQuery(".pt_product-search-result .search-result-found .primary-content").css("width",jQuery(window).width());

	}
	else{
		jQuery(".pt_product-search-result .search-result-found .primary-content").css("width","100%");

	}
	jQuery(".sort-by-dom-label").off("click").on("click",function(){
		var scroll = jQuery(window).scrollTop();

 	    if(scroll>=scrollvalue&&scroll!=0){
			if(jQuery(window).width()<1024){
				jQuery(".mobile-sort").css("top",(jQuery(".show-mobile-sort").offset().top-30)-31);
				jQuery(".mobile-sort").css("left",jQuery(".show-mobile-sort").offset().left-6);
				if(jQuery(".sort-by-dom-label.show-mobile-sort").hasClass("downarrow")){
			    	jQuery("ul.mobile-sort").attr('style',"display:none !important");
					jQuery(this).removeClass("downarrow");

				}
				else{

					jQuery("ul.mobile-sort").attr('style',"display:inline-block;position:fixed;left:"+(jQuery(".show-mobile-sort").offset().left-0)+"px;top:"+(jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).height()+jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result .mobile-category-header.fixed-top").height()-5)+"px;z-index:999");
					jQuery(".sort-by-dom-label.show-mobile-sort").addClass("downarrow");

				}
			}
	    }
		else{
			if(jQuery(".sort-by-dom-label.show-mobile-sort").hasClass("downarrow")){
		    	jQuery("ul.mobile-sort").attr('style',"display:none !important");
		    	jQuery(this).removeClass("downarrow");

			}
			else{
				if(jQuery(window).width()<1024){
					jQuery("ul.mobile-sort").attr('style',"display:inline-block;left:"+(jQuery(".show-mobile-sort").offset().left)+"px;top:"+(jQuery(".search-result-mobile-header").offset().top+jQuery(".search-result-mobile-header").height()-5)+"px;z-index:999");
				}
				else{
					jQuery("ul.mobile-sort").attr('style',"display:inline-block;left:"+(jQuery(".show-mobile-sort").offset().left)+"px;top:"+(jQuery(".search-result-mobile-header").offset().top-15)+"px");

				}

				jQuery(".sort-by-dom-label.show-mobile-sort").addClass("downarrow");

			}


		}
		return false;
	});

	jQuery(".filter-label.show-mobile-filters").on("click",function(){
		jQuery(".refinement-link.active").eq(0).off("click").on("click",function(e){
			  e.preventDefault();
		        e.stopPropagation(); return false;});
		//jQuery(".pt_product-search-result #secondary .white-bg  .refinement.dropdown-products-projects ul li a.active").parent().css("background","#e4e4e4");
		updateMobileMenu();
		jQuery(".mobile-sort").attr("style","display:none !important");


		resizeWindow();

		if(jQuery(window).width()<1024){

			jQuery( ".pt_product-search-result #secondary .white-bg" ).toggle( "slide" );

		}



	});


	if(jQuery(window).width()<1024){
		jQuery(".mobile-sort").html(jQuery("ul#grid-sort-header").html());
    	jQuery("ul.mobile-sort").attr('style',"display:none !important");

	}
	else{
		jQuery(".mobile-sort").html("");
    	if(jQuery(".show-mobile-sort").length>0){
    		jQuery("ul.mobile-sort").attr('style',"display:inline-block;position:fixed;left:"+(jQuery(".show-mobile-sort").offset().left-0)+"px;top:"+(jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).height()+jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result .mobile-category-header.fixed-top").height()-5)+"px;z-index:999");
    	}
    }

  	function gridSet(){
  		if (!isTouchDevice()) {
			setScrollConstant();
			$("html,body").scrollTop(0);

			if(jQuery(window).width()<1024){

				jQuery(".mobile-sort").html(jQuery("ul#grid-sort-header").html());
		    	jQuery("ul.mobile-sort").attr('style',"display:none !important");
		    	jQuery(".pt_product-search-result  div.breadcrumb").css('max-width',jQuery(window).width());
				jQuery(".sort-by-dom-label.show-mobile-sort.downarrow").removeClass("downarrow");
		    	jQuery(".pt_product-search-result #secondary .white-bg").attr('style','display:none');


			}
			else{
				jQuery(".mobile-sort").html("");
		    	jQuery(".pt_product-search-result #secondary .white-bg").attr('style',"display:block");
		    	if(jQuery(".show-mobile-sort").length>0){
		    		jQuery("ul.mobile-sort").attr('style',"display:inline-block;position:fixed;left:"+(jQuery(".show-mobile-sort").offset().left-0)+"px;top:"+(jQuery(".pt_product-search-result #secondary .search-result-mobile-header").eq(0).height()+jQuery(".search-found-container .mobile-search").eq(0).height()+jQuery(".pt_product-search-result .mobile-category-header.fixed-top").height()-5)+"px;z-index:999");

		    	}
		    }
			if(jQuery(window).width()<1024){
		    	jQuery(".pt_product-search-result .search-result-found .primary-content").css("width",jQuery(window).width());

			}
			else{
				jQuery(".pt_product-search-result .search-result-found .primary-content").css("width","100%");

			}

			clearTimeout(doit);
			doit = setTimeout(resizeWindow, 100);
			if($(".shop-filter-type").length==0  ){
				if(jQuery(window).width()>1024){
					$(".filter-label").attr("style","display:none !important");
					$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","margin-left:5px;");
				}
				else	{
					$(".filter-label.show-mobile-filters").attr("style","");
					$(".shop-filter-label.mobile-label").addClass("shop-filter-label-hide");
					$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");

				}
				//$(".shop-filter-label").attr("style","top:8px");
				$(".pt_product-search-result #secondary .white-bg .filter-container br").remove();
			}
			else{
				$(".filter-label").attr("style","");
				$(".shop-filter-label.mobile-label").removeClass("shop-filter-label-hide");
				$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");
			}
		}

  	}

	jQuery(window).resize(function (event) {
		gridSet();

	});
	jQuery(window).on("orientationchange",function (event) {
		if(jQuery(window).width()<1024){
	    	jQuery(".pt_product-search-result .search-result-found .primary-content").css("width",jQuery(window).width());

		}
		else{
			jQuery(".pt_product-search-result .search-result-found .primary-content").css("width","100%");

		}
		updateMobileMenu();
		resizeWindow();


	});
	jQuery(window).scroll(function (event) {
	   		loadScrollPage();
	});


	jQuery(document).on("scrollstop",function(){
		if (isTouchDevice()) {
			$("#prototype-secondary").slideDown();

		}
	});


	//causing error with ajax loading
	jQuery(".sort-by-dom h3 span").html(jQuery(".project-refinement.grid-sort-header a[selected='selected']").html());

	//causing error with ajax loading
	jQuery(".mobile-filter-done.primary").off("click").on("click",function(e){
		  e.preventDefault();
	        e.stopPropagation();
			jQuery(".filter-label.show-mobile-filters").trigger("click");
			setCookie("grid-dropdown-active-class","");
		  return false;

	});
	jQuery(".sort-by label").html(jQuery(".sort-by .sorting-option[selected='selected']").html());



});



</script>
