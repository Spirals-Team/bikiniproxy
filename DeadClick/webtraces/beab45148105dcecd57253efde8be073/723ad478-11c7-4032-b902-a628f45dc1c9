<script>

jQuery(document).ready(function(){
	addGridSwipe();
	updateMobileMenu();

	$( document ).ajaxSend(function( event, jqxhr, settings ) {
		if (isTouchDevice()) {
			if(settings['url'].indexOf("Search-GetSuggestions")==-1 ){
			  if($("#loading-temp").length==0){
				$('body').prepend('<div id="loading-temp" style="position:fixed;height:1000px;width:100%;background:#fff;opacity:0.8;z-index:10000"><div style="width:100px;margin-left: -50px;margin-right: auto;display: block;float: none;left: 50%;top: 200px;border:none;" class="zoomPreload">&nbsp;</div></div>');

			  }
			}
		}

	});
	$( document ).ajaxComplete(function( event,request, settings ) {
		if(getCookie("grid-search-wide")=="true"){
			jQuery(".pt_product-search-result  .search-result-content").addClass("wide-tiles");
			jQuery(document).ready(function(){
				jQuery(".results-hits .toggle-grid").addClass("select-wide-tiles");
				jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible");
			})

		}
		else	{

			if(getCookie("grid-search-wide")=="false"){
				jQuery(".pt_product-search-result  .search-result-content").removeClass("wide-tiles");
				jQuery(document).ready(function(){
					jQuery(".results-hits .toggle-grid").removeClass("select-wide-tiles");
					jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible");
				});
			}
			else	{
				if(jQuery(".pt_product-search-result  .search-result-content").hasClass("wide-tiles")){
					jQuery(document).ready(function(){
						jQuery(".results-hits .toggle-grid").addClass("select-wide-tiles");
						jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible");
					})

				}
			}


		}
		jQuery(".results-hits").mousemove(function(e){
			var offsetValue=e.pageX-jQuery(this).offset().left;
        	if(offsetValue>=0 && offsetValue<=26){
        		jQuery(".toggle-grid").css('cursor','pointer').attr('title', 'Small View');

        	}
        	else if(offsetValue>=30 && offsetValue<=56){
        		jQuery(".toggle-grid").css('cursor','pointer').attr('title', 'Large View');
        	}
        	else{
        		jQuery(".toggle-grid").css('cursor','auto');

        	}

        });

		resizeWindow();
		addGridSwipe();
		loadRefinementCount();
		addGridSlot();
		if(parseInt($(".current_page_search").html())==0){}
	    if(jQuery(window).width()<1024 && (jQuery(getCookie("grid-dropdown-active-class")).find("h3").length>0 || getCookie("grid-dropdown-active-class")=="toggle")){


			jQuery( ".pt_product-search-result #secondary .white-bg" ).show();


	    }

		updateMobileMenu();

		$("#loading-temp").remove();
		$(window).load(function() {
			setScrollConstant();
		});
		setScrollConstant();
		if($(".shop-filter-type").length==0  ){
			if(jQuery(window).width()>1024){
				$(".filter-label").attr("style","display:none !important");
				$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","margin-left:5px;");
			}
			else	{
				$(".filter-label.show-mobile-filters").attr("style","");
				$(".shop-filter-label.mobile-label").addClass("shop-filter-label-hide");
				$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");

			}
			//$(".shop-filter-label").attr("style","top:8px");
			$(".pt_product-search-result #secondary .white-bg .filter-container br").remove();
		}
		else{
			$(".filter-label").attr("style","");
			$(".shop-filter-label.mobile-label").removeClass("shop-filter-label-hide");
			$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");
		}
		loadScrollPage();
		jQuery(getCookie("grid-dropdown-active-class")).find("h3").trigger("click")
		setCookie("grid-dropdown-active-class","");

	});


		if(getCookie("grid-search-wide")=="true"){
			jQuery(".pt_product-search-result  .search-result-content").addClass("wide-tiles");
			jQuery(document).ready(function(){
				jQuery(".results-hits .toggle-grid").addClass("select-wide-tiles");
				jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible")
			})

		}
		else	{

			if(getCookie("grid-search-wide")=="false"){
				jQuery(".pt_product-search-result  .search-result-content").removeClass("wide-tiles");
				jQuery(document).ready(function(){
					jQuery(".results-hits .toggle-grid").removeClass("select-wide-tiles");
					jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible")
				});
			}
			else	{
				if(jQuery(".pt_product-search-result  .search-result-content").hasClass("wide-tiles")){
					jQuery(document).ready(function(){
						jQuery(".results-hits .toggle-grid").addClass("select-wide-tiles");
					})

				}
			}


		}

		jQuery(".results-hits").mousemove(function(e){
			var offsetValue=e.pageX-jQuery(this).offset().left;
        	if(offsetValue>=0 && offsetValue<=26){
        		jQuery(".toggle-grid").css('cursor','pointer').attr('title', 'Small View');

        	}
        	else if(offsetValue>=30 && offsetValue<=56){
        		jQuery(".toggle-grid").css('cursor','pointer').attr('title', 'Large View');
        	}
        	else{
        		jQuery(".toggle-grid").css('cursor','auto');

        	}

        });

		$(window).load(function() {
			if (!isTouchDevice()) {
				setScrollConstant();
			}
		});



		if($(".shop-filter-type").length==0  ){
			if(jQuery(window).width()>1024){
				$(".filter-label").attr("style","display:none !important");
				$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","margin-left:5px;");
			}
			else	{
				$(".filter-label.show-mobile-filters").attr("style","");
				$(".shop-filter-label.mobile-label").addClass("shop-filter-label-hide");
				$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");

			}
			//$(".shop-filter-label").attr("style","top:8px");
			$(".pt_product-search-result #secondary .white-bg .filter-container br").remove();
		}
		else{
			$(".filter-label").attr("style","");
			$(".shop-filter-label.mobile-label").removeClass("shop-filter-label-hide");
			$(".pt_product-search-result #secondary .white-bg .filter-container").attr("style","");
		}
		addGridSlot();
		setScrollConstant();
		jQuery(".pt_product-search-result ul.search-result-items").css("visibility","visible");
});
