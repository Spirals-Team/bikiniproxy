jQuery(document).ready(function(){
	//setCookie("grid-dropdown-active-class","")

	jQuery(".refinement h3:not(.projectproducts-title)").off("click").on("click",function(event){
		
		 event.stopImmediatePropagation(); 
		 // event.stopPropagation();
		jQuery(this).parent().siblings().find(".dropopen").removeClass("dropopen");
		jQuery(this).parent().siblings().find(".downarrow").removeClass("downarrow");
		if(jQuery(this).parent().find("ul.dropopen").length>0){
			jQuery(this).parent().find("ul").removeClass("dropopen");
			jQuery(this).parent().find("h3").removeClass("downarrow");
			jQuery(this).parent().find("h2").removeClass("downarrow");
		}
		else{

			jQuery(this).parent().find("ul").addClass("dropopen");
			jQuery(this).parent().find("h3").addClass("downarrow");
			jQuery(this).parent().find("h2").addClass("downarrow");
		}
		return false;
	})
	jQuery(".category-refinement h2.refinement-header").eq(0).off("click").on("click",function(event){
		 event.stopImmediatePropagation();
		 // event.stopPropagation();
		jQuery(this).parent().siblings().find(".dropopen").removeClass("dropopen");
		jQuery(this).parent().siblings().find(".downarrow").removeClass("downarrow");
		if(jQuery(this).parent().find("ul.dropopen").length>0){
			jQuery(this).parent().find("ul#category-level-1").removeClass("dropopen");
			jQuery(this).parent().find("h3").removeClass("downarrow");
			jQuery(this).parent().find("h2").removeClass("downarrow");
		}
		else{
			jQuery(this).parent().find("ul#category-level-1").addClass("dropopen");
			jQuery(this).parent().find("h3").addClass("downarrow");
			jQuery(this).parent().find("h2").addClass("downarrow");
		}
		return false;
	})
	if(jQuery(".show-mobile-sort").length==0){
		jQuery(".mobile-sort").remove();
		
	}
});
