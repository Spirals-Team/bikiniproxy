jQuery('.esg-filter-wrapper.blackred .selected').addClass("wasselected");
jQuery('body').on('click','.esg-filter-wrapper.blackred .esg-filterbutton',function() {
  	var mfb = jQuery(this),
    	fid = mfb.data('fid');
 	jQuery('.esg-filter-wrapper.blackred .wasselected').removeClass("wasselected");
    mfb.addClass("wasselected");
  jQuery('.esg-filter-wrapper.whiteblack').find('.esg-filterbutton').each(function() {
     var fb = jQuery(this);
    if (fid==fb.data('pid') || fid=="-1" || mfb.hasClass("hiddensearchfield")) {
        fb.removeClass('hidefilter');
    } else {
        fb.addClass('hidefilter');
    }
  });
});});
