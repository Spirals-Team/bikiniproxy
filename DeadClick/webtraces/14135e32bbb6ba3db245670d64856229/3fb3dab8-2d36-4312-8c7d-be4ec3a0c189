jQuery.fn.hammer=function(a){return this.each(function(){var b=new Hammer(this,a);var d=jQuery(this);d.data("hammer",b);var c=["hold","tap","doubletap","transformstart","transform","transformend","dragstart","drag","dragend","swipe","release"];for(var f=0;f<c.length;f++){b["on"+c[f]]=(function(g,e){return function(h){g.trigger(jQuery.Event(e,h));};})(d,c[f]);}});};