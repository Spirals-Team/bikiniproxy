  <script type="text/javascript">
    var PREBID_TIMEOUT = PREBID_TIMEOUT || 600;
    var USE_PREBID = USE_PREBID || true;

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
        
    function initAdserver() {
        if (typeof pbjs !== 'undefined' &&  pbjs.initAdserverSet) return;
        //load GPT library here
        var gads = document.createElement('script');
        gads.async = true;
        gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') +
        '//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
        
        if (typeof pbjs !== 'undefined') {
            pbjs.initAdserverSet = true;
        }
    };

    if (USE_PREBID) {
        setTimeout(initAdserver, PREBID_TIMEOUT);

        var AdCount = 2;
      
        var pb_adUnits = prebid_dt.slice(0, AdCount);  // prebid_dt contains the json ad units for prebid.
        var pbjs = pbjs || {};
        pbjs.que = pbjs.que || [];

        var pbjsEl = document.createElement("script"); pbjsEl.type = "text/javascript";
        pbjsEl.async = true; var isHttps = 'https:' === document.location.protocol;
        pbjsEl.src = (isHttps ? "https://" : "http://") + "zdstatic.toolbox.com/pb/pbl.min.js";
        var pbjsTargetEl = document.getElementsByTagName("head")[0];
        pbjsTargetEl.insertBefore(pbjsEl, pbjsTargetEl.firstChild);

        pbjs.que.push(function() {
            pbjs.addAdUnits(pb_adUnits);
            pbjs.requestBids({
                bidsBackHandler: function(bidResponses) {
                    initAdserver();
                    // remove this comment from production
                    // call coreDMP with every bid returned -- both winners and losers.  
                    // "bsid" is a guid that is generated so we know all the bids that came back in the same request.
                    try {
                        for (var bidadslot  in bidResponses) {
                            var bsid = function b(a){ return a?(a^Math.random()*16>>a/4).toString(16):([1e7]+-1e3+-4e3+-8e3+-1e11).replace(/[018]/g,b)}(); //guid generator
                            if (bidResponses.hasOwnProperty(bidadslot)) {
                                for (var i=0;i < bidResponses[bidadslot].bids.length;i++) {
                                  try {
                                    var b = bidResponses[bidadslot].bids[i];
                                    var s = new Image(1,1);
                                    var size="NA";
                                    var hb_pb=0;
                                    if (b.cpm != 0) {  // yes bid
                                        hb_pb = b.adserverTargeting.hb_pb;
                                        size = b.size;
                                    }
                                    s.src = "//zdbb.net/l/EBd3N5GSEeWA3CIAC7Ppng?bidder=" + b.bidderCode + "&provider=prebid&site=toolbox.com&cpm=" + b.cpm + "&bsid="+ bsid + "&bid=" + hb_pb+ "&adid=" + b.adId + "&adunit=" + b.adUnitCode + "&size=" + size + "&ttr=" + b.timeToRespond +"&status=" + encodeURIComponent(b.statusMessage);
                                    //console.log(s.src);
                                  } catch(err) { }
                                }
                             }
                        }
                    } catch(err) {  console.log('prebidlog err');}
                },
            })

        });

        pbjs.bidderSettings = {
            standard: {
                alwaysUseBid: false,
                adserverTargeting: [{
                    key: "hb_bidder",
                    val: function(bidResponse) {
                        return bidResponse.bidderCode;
                    }
                }, {
                    key: "hb_adid",
                    val: function(bidResponse) {
                        return bidResponse.adId;
                    }
                }, { 
                    key: "hb_pb",
                    val: function(bidResponse) {
                             if (bidResponse.cpm > 200) { //10$
                                return (Math.floor(Math.min(parseInt(bidResponse.cpm), 500) / 10) * 10).toFixed(2);
                              } else
                              if (bidResponse.cpm > 40) { //1$
                                return (Math.floor(bidResponse.cpm)).toFixed(2);
                              } else
                              if (bidResponse.cpm > 30) { // 10c
                                return (Math.floor(bidResponse.cpm * 10) / 10).toFixed(2);
                              }
                              return bidResponse.cpm.toFixed(2); 
                          }
                }, {
                    key: "hb_size",
                    val: function(bidResponse) {
                        return bidResponse.size;
                    }
                }, {
                    key: "hb_cpm",
                    val: function(bidResponse) {
                        return bidResponse.cpm;
                    }
                },{
                     key: "hb_dealid",
                     val: function(bidResponse) { return (typeof bidResponse.dealId === 'undefined' ? "0" : bidResponse.dealId); }
                  }
               ]
            } 
         }
    } else {
        initAdserver();
    }

(function(){try{!function(window,document,$){window.top.stitial=stitial={init:function(options){options.name=this.getAdContainerDivId(options)||"bishop",stitial.options=options,options.$stitialAdContainer=$("#"+options.name),options.$stitialAdContainer.after(options.domString),options.$stitialRootEl=$("#stitialv2"),options.$stitialCreative=$("#stitialv2 .creative"),options.$stitialFuseWire=$("#stitialv2 .fuse-wire"),options.impURLs.forEach(function(element){""!==element&&((new window.Image).src=element+(-1===element.indexOf("?")?"?":-1===element.indexOf("http",3)?"&":"?")+"nocache="+(new Date).getTime())}),$("body",document).css("margin","0px"),options.$stitialCreative.css({width:options.creativeWidth+"px",height:options.creativeHeight+"px"}),stitial.pageSetup();var adCode="";if("Local_SWF"===options.creativeType&&options.localSwfUrl){options.allowScriptAccess||(allowScriptAccess="never");var swfobjectCode='\x3c!--[if IE]>\n<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="100%" height="100%" id="'+name+'""><param name="movie" value="'+options.localSwfUrl+'" />\n<![endif]--\x3e\n';swfobjectCode+='\x3c!--[if !IE]>--\x3e\n<object type="application/x-shockwave-flash" data="'+options.localSwfUrl+'" width="100%" height="100%" id="'+name+'"">\n\x3c!--<![endif]--\x3e\n',swfobjectCode+='<param name="flashvars" value="clickTag='+escape(options.clickThroughUrl)+"&clickTAG="+escape(options.clickThroughUrl)+"&clicktag="+escape(options.clickThroughUrl)+'" /><param name="wmode" value="'+options.opacity+'" /><param name="autostart" value="true" /><param name="quality" value="autohigh" /><param name="allowScriptAccess" value="'+options.allowScriptAccess+'" />\n',swfobjectCode+='<a href="'+options.clickThroughUrl+'" target="_blank"><img src="'+options.localImgUrl+'" border="0px" alt="" /></a>',adCode=swfobjectCode+="</object>\n"}else"Local_IMAGE"===options.creativeType&&options.localImgUrl&&(adCode='<a href="'+options.clickThroughUrl+'" target="_blank"><img src="'+