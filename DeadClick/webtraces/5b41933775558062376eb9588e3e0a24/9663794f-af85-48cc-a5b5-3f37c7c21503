				thumbBlock = boxes[i].firstChild.firstChild,
				labelsOverlay = document.createElement('span');
			
			labelsAnchor.className += 'video-labels-anchor';
			if (TRC.Browser.ie === 8) {
				labelsAnchor.className += ' ie8fix';
			}
			boxes[i].removeChild(labelsAnchor);
			labelsOverlay.className = 'label-box-overlay';
			labelBox.appendChild(labelsOverlay);
			thumbBlock.insertBefore(labelsAnchor, thumbBlock.firstChild);
			
			/** Remove video cubes tooltips (link titles)
				- we are doing this here and not in item-renderer since we want to include ads. by
				default, Ads are not included in item-renderer. Changing this will affect all modes of the current version. **/
			var videoAnchors = boxes[i].getElementsByTagName('a');
			for (j = 0; j < videoAnchors.length; j++) {
				videoAnchors[j].removeAttribute('title');
			}
		}
	}
,"item-renderer":function(box,data) {
        if(box.rbox.boxes.length%2 == 0){
            box.className += ' firstInRow';
        }

		//Append the thumbBlock to an additional div so we can constrain its width using "max-width" in css
		var constrainDiv = document.createElement('div');
		constrainDiv.className += ' thumbConstrain';
		constrainDiv.appendChild(box.thumb);
		box.firstChild.appendChild(constrainDiv);

		//Resize and crop thumb image using margins. Basically does the same as fixVideoImage() but with margins in %
		var imgOnLoad = box.img.onload;
		box.img.onload = function() {
			var minHeight = 116,
				minWidth = 174,
				desiredAspectRatio = minHeight/minWidth;
				imgHeight = box.img.clientHeight,
				imgWidth = box.img.clientWidth,
				aspectRatio = imgHeight / imgWidth,
				isVerticalFixRequired = imgHeight < minHeight, //if the image's height is smaller than it should be, we will fix the height here - so we need to fix the offset horizontally. otherwise, this means that rbox's fixVideoImage() enlraged the width, so the offset should be fixed vertically.
				offsetRatio = isVerticalFixRequired ? 
								(imgHeight - minHeight) / minHeight : //horizontal offset ratio (change in size / desired height)
								(minHeight - imgHeight) / minWidth; //vertical offset ratio (change in size / desired width)

			if(typeof imgOnLoad === 'function') {
				imgOnLoad();
			}

			if(isVerticalFixRequired) { //Enlarge thumbnail height and width by percent
				box.thumb.style.height = box.thumb.style.width = minHeight / imgHeight * 100 + '%';
			}else { //Image width was fixed by rbox, simply whange thumb height to "auto" (instead of values in px)
				box.thumb.style.height = box.thumb.style.width = 'auto';
			}

			//Fix image offset by setting negative margins in percent
			if(aspectRatio > desiredAspectRatio) {
				box.img.style.marginTop = box.img.style.marginBottom = offsetRatio * 100 / 2 + '%';
			}else if(aspectRatio < desiredAspectRatio) {
				box.img.style.marginLeft = box.img.style.marginRight = offsetRatio * 100 / 2 + '%';
			}

			box.img.style.visibility = 'visible';
		};
