	if(TRC.publisherId.indexOf('unknown-site') >= 0 || TRC.publisherId == 'timeinc-time') {
		TRC.publisherId = TRC.publisherId.replace('unknown-site-on-', '');
	
		if (window.location.toString().match(/timeinc\.net/)) { 
			window.TRC.publisherId = 'timeinc-time';
			return;
			/* Fixme: change this to better code  this was QND */
		}
		
		/** Returns domain name from FQDN; example: 'subdomain.domain.com' => 'domain'. This function assumes
		 * a top level domain of com/info/org/net and will not work for URLs that end with .co.uk and similar ones. */
		var getHostFromFqdn = function(url) {
				var regex = /^\/?([^:\/\s]+)*\.(com|info|org|net)$/;
				if ((url = url.match(regex)).length === 3) {
					return url[1].split('.').pop();
				}
				return false;
			},
			host = getHostFromFqdn(window.location.host.toLowerCase()),
			currentPublisherId = window.TRC && TRC.publisherId,
			networkPublisherIdPrefix = currentPublisherId && currentPublisherId.split('-').shift();
		/** The cross network deplolyment requires the publisherId to be set to 'timeinc-domain'.
		 * On 'taboola.com' we fallback to 'timeinc-time' to get some recommendations. */
		currentPublisherId && (window.TRC.publisherId = networkPublisherIdPrefix + '-' + (host === 'taboola' ? 'time' : host));
	}

