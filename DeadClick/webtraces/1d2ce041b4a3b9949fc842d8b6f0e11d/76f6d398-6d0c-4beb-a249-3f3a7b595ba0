<script>
    var league = 'NFL';
    var teamName = 'New_Orleans_Saints';
    var pageNewArrivals = '/pages/New_Arrivals?ab=bm-nflcms-hp-Header1-NewArrivals-2.7';
    var pageCustomShop = '/Custom_Shop?ab=bm-nflcms-hp-Header2-CustomShop-2.7';
    var pageOutlet = '/pages/Outlet?ab=bm-nflcms-hp-Header3-Outlet-2.7';
    var tmNewItems = "/" + teamName + "/so/newest_items";
    var tmCustomShop = "/" + teamName + "_Custom_Shop";
    var tmOutlet = "/" + teamName + "/on_sale/yes";
    
    if (!teamName) {
      jQuery('a.new-arrivals-container').attr('href',pageNewArrivals);
      jQuery('a.custom-shop-container').attr('href',pageCustomShop);
      jQuery('a.outlet-container').attr('href',pageOutlet);           
    } else {      
      jQuery('a.new-arrivals-container').attr('href', tmNewItems);
      jQuery('a.custom-shop-container').attr('href', tmCustomShop);
      jQuery('a.outlet-container').attr('href', tmOutlet);
    }   
</script><!-- End Content Text ID: 1813880 and Content Target ID: 2113297 -->
                
