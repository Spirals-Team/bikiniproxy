<script>
function rr_callbackTeam(placements) {
    if ((typeof placements !== "undefined") && (typeof tempPlayerList !== "undefined") && (typeof tempPlayerList.TeamData !== "undefined")) {
        var rr_teamPlacement = jQuery.grep(placements, function (b) { return typeof b.placement_name !== "undefined" && b.placement_name.indexOf("nav_team") > 0; });
        if ((typeof rr_teamPlacement !== "undefined") && (typeof rr_teamPlacement[0] !== "undefined")) {
            var rr_players = jQuery.grep(tempPlayerList.TeamData, function (e) { return e.team === rr_teamPlacement[0].team_name; });
            var rr_playerList = new PlayerList();
            var rr_shopByTeamElement = jQuery("#nav-shop-by-team-home");
            var rr_jerseysElement = jQuery("#nav-jerseys-home");

            if (typeof rr_players !== "undefined" && typeof rr_players[0] !== "undefined" && rr_shopByTeamElement.length) {
           var rr_teamLogo = "<a href=\"" + rr_players[0].teamInfo.url + "\"><img src=\"" + rr_players[0].teamInfo.logoUrl + "\" /></a>";
                TCS.TeamData = null;
                rr_playerList.renderPlayers(rr_players[0], rr_shopByTeamElement);
               rr_shopByTeamElement.prev().html(rr_teamLogo);
            }
            if (typeof rr_players !== "undefined" && rr_jerseysElement.length) {
           var rr_teamLogo = "<a href=\"" + rr_players[0].teamInfo.url + "\"><img src=\"" + rr_players[0].teamInfo.logoUrl + "\" /></a>";
                TCS.TeamData = null;
                rr_playerList.renderPlayers(rr_players[0], rr_jerseysElement);
               rr_jerseysElement.prev().html(rr_teamLogo);
            }
        }
    }
}
</script>

