        .top-bar .top-bar-section>ul>li.navSweatshirtsTeam:hover:not(.has-form):not(.name)>a:after{left: 43%;  }
          #navHatsTeam { float: left; width: 7.27274%;  }
        .top-bar .top-bar-section>ul>li.navHatsTeam:hover:not(.has-form):not(.name)>a:after{left: 40%;  }
          #navCollectiblesTeam { float: left; width: 10.12988%;  }
        .top-bar .top-bar-section>ul>li.navCollectiblesTeam:hover:not(.has-form):not(.name)>a:after{left: 43%;  }
          #navHomeOfficeTeam { float: left; width: 10.12988%;  }
        .top-bar .top-bar-section>ul>li.navHomeOfficeTeam:hover:not(.has-form):not(.name)>a:after{left: 43%;  }
          #navTailgatingTeam { float: left; width: 10.12888%;  }
        .top-bar .top-bar-section>ul>li.navTailgatingTeam:hover:not(.has-form):not(.name)>a:after{left: 43%;  }
          
    }

    
               #navNFLTLP > .dropdown {width: 80%;min-width: 80%;left:0%}

               #navJerseysTeam > .dropdown {width: 62%;min-width: 62%;left:0%}

