era")?b.includes("opera")?c=a.substring(a.indexOf("Opera")).split(" ")[0].split("/")[0]+"-"+a.substring(a.indexOf("Version")).split(" ")[0].split("/")[1]:b.includes("opr")&&(c=a.substring(a.indexOf("OPR")).split(" ")[0].replace("/","-").replace("OPR","Opera")):b.includes("chrome")?c=a.substring(a.indexOf("Chrome")).split(" ")[0].replace("/","-"):b.includes("firefox")&&(c=a.substring(a.indexOf("Firefox")).split(" ")[0].replace("/","-"));return c};