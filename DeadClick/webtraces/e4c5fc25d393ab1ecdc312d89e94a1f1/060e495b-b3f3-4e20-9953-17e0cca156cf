        var utils = optimizely.get('utils');
var $ = optimizely.get('jquery');
var hostname = window.location.hostname;

// execute value
var execute = (function(mode){
  var res = false;
  // only execute if mode=live
  if (mode === 'live') {
    res = true;
  // or if there is the query parameter _pviewtest=1  
  } else if (/_pviewtest=1/.test(window.location.search) || /optimizelyedit/.test(hostname)) {
    res = true;
  }
  return res;
})(extension.extMode);

log('host:', hostname);
log('execute:', execute);
// don't execute if false
if (!execute) { return; }

// extension div id
var id ='optimizely-extension-' + extension.$instance;

// cookie properties (opt in/out)
var cookie = {
  name: '_fxn_opt-pvvfnn',
  inSampleName: '_fxn_opt-psvfnn',
  path: '/',
  domain: '.foxnews.com',
  val: {
    accept: 'Y0hKdmNERT0=',
    deny: 'Ym05MFlXTmpaWEIw'
  },
  optOutHrs: extension.optOutExpireInHrs
};

var cookieVal = getExperimentCookie();
// if the cookie is not "accept" set it
if (cookieVal !== 'accept') {
  cookieVal = setAccept(true);
}

// if user was part of sample
var isSample = getCookie(cookie.inSampleName) ? true : false;
// fire tracking for direct url visitors
if (!isSample) {
  track('direct');
}
// set custom attribute for NR reporting
triggerNR(isSample ? 'sample' : 'direct');

utils.waitForElement('#wrapper').then(function(elem) {
    elem.insertAdjacentHTML('afterbegin', extension.$html);
  initialize();
});

function initialize() {
  var target = $('#' + id),
      p = target.find('> p');
  
  // process text
  p.html(p.html()
         .replace(/\[\[(.+)\]\]/, '<a class="rate" target="_blank" href="https://foxnews.typeform.com/to/O4mBT8" data-omtr-intcmp="hp1opto_2">$1</a>')
         .replace(/({{(.+)}})/, '<a class="back" href="//www.foxnews.com" data-omtr-intcmp="hp1opto">$2</a>'));
  
    p.removeClass('hide');
  
  // go back to the current homepage
  target.find('a.back').on('click', function(evt){
    evt.preventDefault();
    // for users in the sample, who accepted it
    if (cookieVal && cookieVal === 'accept') {
      // fire tracking stop
      var trackEvent = (isSample) ? 'stop' : 'stop-direct';
      log('triggerEvt:', trackEvent);
      track(trackEvent, function(){
        // set the cookie as 'deny'
        setAccept(false);
        next();
      });
    } else {
        next();
    }
    function next() {
        //rate();
        redirect();
    }
    
    return false;
  });
  
  // close the overlay
  target.find('a.close').on('click', function(evt){
    evt.preventDefault();
    target.remove();
    return false;
  });
  
  // rate
  target.find('a.rate').on('click', function(evt){
    evt.preventDefault();
    rate();
    return false;
  });

  // show overlay
  target.removeClass('hide');
}

// set cookie value
function setAccept(bool) {
  // alert optimizely tester
    if (!/foxnews\.com$/.test(hostname)) {
    //alert('no cookie will be set, because you are in testing');
    return;
  }
    log('Setting:', (bool) ? 'accept' : 'deny');
  var val = (bool) ? cookie.val.accept : cookie.val.deny;
  var expires = (function(){
    // default expire @ 1 year (for those who accepted)
    var res = 365;
    // if user opts out
    if (!bool) {
      // show it again after x hours
      var hrs = (!isNaN(cookie.optOutHrs)) ? (parseInt(cookie.optOutHrs, 10)) : 6; // default 6 hrs
      var after = 1000 * 60 * 60 * (hrs || 6);
      res = new Date((new Date()).getTime() + after);
    }
    return res;
  })();
  var props = { path: cookie.path, domain: cookie.domain, expires: expires };
  log('setting cookie:', cookie.name, val, props);
  $.cookie(cookie.name, val, props);
  return val;
}

// get cookie value
function getExperimentCookie() {
  var val = getCookie(cookie.name);
  var res = (val === cookie.val.accept) ? 'accept' : (val === cookie.val.deny) ? 'deny' : null;
  log('get cookie val:', res);
  return res;
}

function getCookie(name) {
  return $.cookie(name) || null;
}

// redirect to preview page
function redirect() {
  window.open('//www.foxnews.com/', '_self');
}

// rate it
function rate() {
  triggerNR('rate', true, function(){
    window.open('https://foxnews.typeform.com/to/O4mBT8', '_blank');
  });
}

// Conditional output.
function log() {
  var show = (/(_logs=1|_pviewtest=1)/.test(window.location.search)) ? true : false;
  if(show) {
    var args = Array.isArray(arguments) ? arguments : [].slice.call(arguments);
    args.unshift('[fn.preview.ext]');
    console.info.apply(null, args);
  }
}

// tracking
function track(val, callback) {
  var name = null;
  
  switch(val) {
    case 'stop':
      name = 'hppreview-stop';
      break;
    case 'stop-direct':
      name = 'hppreview-stop-direct';
      break;
    case 'direct':
      name = 'hppreview-direct';
      break;
  }
  
  if (name) {
      optimizely.push({
        type: "event",
        eventName: name
      });
      if (val !== 'direct') {
        triggerNR(val, true, callback);
      }
  }
}

function triggerNR(val, evt, callback) {
  try {
    var provider = FNC.ISA.provider('new-relic');
    if (typeof provider.setCustomAttributes === 'function') {
      if (evt) {
        provider.setCustomAttributes({ previewFncEvent: val });
      } else {
        provider.setCustomAttributes({ previewFncVisitor: val });
      }
    }
  } catch(err) {
    // fail silently
  }
  if (typeof callback === 'function') {
    setTimeout(function(){
      callback();
    }, 0);
  }
  
}
      },
      hideFn: function(event) {
        var $ = window.optimizely.get('jquery');
        var widget = event.data.config;
        widget.$id = "8489141430";
        widget.$instance = event.data.id;
        widget.$fieldDefaults = [{"name":"extMode","default_value":"live"},{"name":"languageText","default_value":"Welcome to the new look FoxNews.com. What do you think? You can [[send us your feedback]]."}];
