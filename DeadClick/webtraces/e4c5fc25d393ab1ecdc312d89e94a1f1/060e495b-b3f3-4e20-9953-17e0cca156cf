// extension div id
var id ='optimizely-extension-' + extension.$instance;
// cookie properties
var cookie = {
  name: '_fxn_opt-pvvfnn',
  path: '/',
  domain: '.foxnews.com',
  val: {
    accept: 'Y0hKdmNERT0=',
    deny: 'Ym05MFlXTmpaWEIw'
  },
  optOutHrs: 6
};

// set sample cookie
(function(){
  var name = '_fxn_opt-psvfnn';
  var val = getCookie(name);
  if (!val) {
    var props = { path: '/', domain: cookie.domain, expires: 365 };
    $.cookie(name, 'YzIxdw==', props);
  }
})();

utils.waitForElement('body').then(function(elem) {
  var cookieVal = getExperimentCookie();
  // if there's a cookie, assuming that the user has opted out
  // do not show the overlay
 	log('cookie value:', cookieVal);
  if (cookieVal === 'deny') {
    log('user has opted out');
    return;
  } else if (cookieVal === 'accept') {
    log('user has already opted in!');
    log('todo: edge redirect');
    // if we can't do the edge redirect, we can do this
    //redirect();
    return;
  }
 
	elem.insertAdjacentHTML('afterbegin', extension.$html);
  initialize();
});

function initialize() {
  var target = $('#' + id);
  
  // set cookie on opt in, then redirect to new page
  target.find('a.launch').on('click', function(evt){
    log('user opt-in');
    setAccept(true);
    track('opt-in');
    redirect();
    return false;
  });
  
  // remove overlay on opt out
  target.find('a.close').on('click', function(){
    target.remove();
    track('opt-out');
    setAccept(false);
    return false;
  });

  // show overlay
  target.removeClass('hide');
}

// set cookie value
function setAccept(bool) {
  
  // alert optimizely tester
	if (!/foxnews\.com$/.test(hostname)) {
    //alert('no cookie will be set, because you are in testing');
    return;
  }
	log('Setting:', (bool) ? 'accept' : 'deny');
  var val = (bool) ? cookie.val.accept : cookie.val.deny;
  var expires = (function(){
    // default expire @ 1 year (for those who accepted)
    var res = 365;
    // if user opts out
    if (!bool) {
      // show it again after x hours
      var hrs = (!isNaN(cookie.optOutHrs)) ? (parseInt(cookie.optOutHrs, 10)) : 6; // default 6 hrs
      var after = 1000 * 60 * 60 * (hrs || 6);
      // on testing, make it 15 seconds
      if (extension.extMode === 'testing') {
        after = 1000 * 15;
      }
      res = new Date((new Date()).getTime() + after);
    }
    return res;
  })();
  var props = { path: cookie.path, domain: cookie.domain, expires: expires };
  log('setting cookie:', cookie.name, val, props);
  $.cookie(cookie.name, val, props);
}

// get cookie value
function getExperimentCookie() {
  var val = getCookie(cookie.name);
  var res = (val === cookie.val.accept) ? 'accept' : (val === cookie.val.deny) ? 'deny' : null;
  log('get cookie val:', res);
  return res;
}

function getCookie(name) {
  return $.cookie(name) || null;
}

// redirect to preview page
function redirect() {
  var redir = '//preview.foxnews.com/index.html';
  log('redirecting to:', redir);
  window.open(redir, '_self');
}

// Conditional output.
function log() {
  var show = true; // (/(_logs=1|_pviewtest=1)/.test(window.location.search)) ? true : false;
  if(show) {
    var args = Array.isArray(arguments) ? arguments : [].slice.call(arguments);
    args.unshift('[fn.preview.ext]');
    console.info.apply(null, args);
  }
}

// tracking
function track(val) {
  var name = null;
  switch (val) {
    case 'opt-in':
      name = 'hppreview-accept';
      break;
    case 'opt-out':
      name = 'hppreview-notaccept';
      break;
  }
  if (name) {
      triggerNR(val);
      optimizely.push({
        type: "event",
        eventName: name
      });
  }
}

function triggerNR(val, id) {
  try {
    var provider = FNC.ISA.provider('new-relic');
    if (typeof provider.setCustomAttributes === 'function') {
      var obj = {};
      obj[id || 'previewFncSampled'] = val;
      provider.setCustomAttributes(obj);
    }
  } catch(err) {
    // fail silently
  }
}
