        var utils = optimizely.get('utils');
var $ = optimizely.get('jquery');
var hostname = window.location.hostname;
var rateUrl = 'https://foxnews.typeform.com/to/qUCE7H';

utils.waitForElement('body').then(function(elem) {
    var aurora = ($('meta[name="hp_version"]').attr('content') === 'aurora') ? true : false;
    var meta = $('meta[name="prism.subsection1"]');
  	if (aurora) {
        cont();
    } else if (meta.attr('content') === 'root-preview') {
        nr({ previewFncForce: 'yes' });
        meta.attr('content', 'root-preview-forced');
        // var title = $('title:first').text();
        // $('title:first').text(title.replace('Fox News - Preview -', 'Fox News -'));
        cont();
    } else {
        nr({ previewFncForce: 'no' });
    }
});

function cont() {
    utils.waitForElement('#wrapper').then(function(elem) {
      elem.insertAdjacentHTML('afterbegin', extension.$html);
      initialize();
    });
}

function initialize() {
  // extension div id
    var id ='optimizely-extension-' + extension.$instance;

    var target = $('#' + id),
        p = target.find('> p');
    // process text
    p.html(p.html().replace(/\[\[(.+)\]\]/, '<a class="rate" target="_blank" href="'+rateUrl+'" data-omtr-intcmp="hp1rate1">$1</a>'));

    p.removeClass('hide');

    // close the overlay
    target.find('a.close').on('click', function(evt){
      evt.preventDefault();
      target.remove();
      return false;
    });

    // rate
    target.find('a.rate').on('click', function(evt){
      evt.preventDefault();
      rate();
      return false;
    });

    // show overlay
    target.removeClass('hide');
}

// rate it
function rate() {
  nr({ 'previewFncEvent': 'rate' }, function(){
    window.open(rateUrl, '_blank');
  });
}

// Conditional output.
function log() {
  var show = true; // (/(_logs=1|_pviewtest=1)/.test(window.location.search)) ? true : false;
  if(show) {
    var args = Array.isArray(arguments) ? arguments : [].slice.call(arguments);
    args.unshift('[fn.preview.ext]');
    console.info.apply(null, args);
  }
}

function nr(data, callback) {
  try {
    var provider = FNC.ISA.provider('new-relic');
    if (typeof provider.setCustomAttributes === 'function') {
      provider.setCustomAttributes(data);
    }
  } catch(err) {
    // fail silently
  }
  if (typeof callback === 'function') {
    setTimeout(function(){
      callback();
    }, 0);
  }

}


function getCookie(name) {
    var match = document.cookie.match(name+'=([^;]*)');
  return match ? match[1] : undefined;
}

