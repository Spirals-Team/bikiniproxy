        var $ = window.optimizely.get('jquery');
        var widget = event.data.config;
        widget.$id = "butter_bar";
        widget.$instance = event.data.id;
        widget.$fieldDefaults = [{"name":"headline","default_value":""},{"name":"cta","default_value":""},{"name":"days","default_value":1}];
