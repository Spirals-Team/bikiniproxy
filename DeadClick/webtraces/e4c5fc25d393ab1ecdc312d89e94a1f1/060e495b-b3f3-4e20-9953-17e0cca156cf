/**
 * If user is authed according to localStorage, set their dimension
 */
(function () {
  var _authed = false;
  try {
    var profileData = JSON.parse(localStorage.getItem("janrainCaptureProfileData"));
    if (profileData && profileData.uuid) {
      _authed = true;
    }
  } catch (c) {}
  if (_authed) {
    window.optimizely.push({
      type: "user",
      attributes: {
        has_account: "yes"
      }
    });
  }
})();

/**
 * Function that fires when the optimizely p13n lifecycle is initialized
 */
window.optimizely.push({
  "type": "addListener",
  "filter": {
    "type": "lifecycle",
    "name": "initialized"
  },
  "handler": jQueryRequired
});

/* jshint latedef:false */
// Modules that require jQuery.
function jQueryRequired() { // jshint ignore:line

  var $ = window.optimizely.get('jquery'); // jshint ignore:line

  /**
   * Track video events back to optimizely
   */
  (function () {
    var events = 'ads_ended,advance,ended,pause,playTriggered'.split(',');
    $(window).on('message', function (event) {
      // try catch name data from amp events
      var name, uid;
      try {
        name = event.originalEvent.data.type;
        uid = event.originalEvent.data.data.uid;
      } catch (e) {}
      // if event is in supported tracked list then fire to optimizely
      if (/^uid-embed-/.test(uid) && $.inArray(name, events) >= 0) {
        window.optimizely.push({
          type: "event",
          eventName: "projectjs-video-" + name
        });
      }
    });
  })();

  /* AdBlock Goal tracking. */
  $(window).load(function () { /* global FNC */
    // If the user has adblock enabled add has-adblock attribute.
    var hasAdBlock = FNC.ISA.provider("coreTracker").isEnabled();
    // based on has or not has adblock fire dimensions and/or track disable rate
    if (hasAdBlock) {
      window.optimizely.push({
        "type": "user",
        "attributes": {
          "has-adblock": "has-adblock",
        }
      });
      window.optimizely.push({
        type: "event",
        eventName: "projectjs-has-adblock"
      });
    } else {
      // they don't have ad block but see if they used
      // to have it enabled and then fire goal
      var visitor = window.optimizely.get('visitor');
      if(
        visitor && visitor.custom && visitor.custom["8262861295"] &&
        visitor.custom["8262861295"].value === "has-adblock"
      ) {
        window.optimizely.push({
          type: "event",
          eventName: "projectjs-disabled-adblock",
        });
      }
    }
  });

}
