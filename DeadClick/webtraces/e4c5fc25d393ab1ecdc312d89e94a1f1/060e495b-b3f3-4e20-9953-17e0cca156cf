var days = parseInt(extension.days, 10) || 1;

// if butter bar was closed in last X then hide
if (lastXDays('adblock_banner_closed', days)) {
  $('head').append('<style>div[data-campaign-id="butter-1"]{visibility: hidden!important;display: none!important}</style>');
}

(function(Campaigns) {
  if (!Campaigns) { return false; }

  Campaigns.setAttr("butter-1", {
    // when user closes the alert banner
    onClose: function() {
      log("POPUP CLOSED!");
      optimizely.push({
        type: "event",
        eventName: "adblock_banner_closed"
      });
    },
    // when user clicks on allow whitelisting
    onAllow: function() {
      log("ALLOW WHITELIST BUTTON CLICKED");
      optimizely.push({
        type: "event",
        eventName: "adblock_banner_clicked_cta"
      });
    },
    // when campaign markup is loaded
    onLoad: function($element) {
      // returns the jQuery element of the popup markup as an argument
      var $headline = $element.find('.content p');
      $headline.text(extension.headline);

      var $cta = $element.find('.content .button a');
      $cta.text(extension.cta);
    }
  });
}(window.FNC.ISA.provider("optimizely.campaigns")));

/*jshint latedef:false*/
function lastXDays(arg1, arg2) {
  'use strict';

  // things interchange

  var days = typeof arg1 === 'number' ? arg1 : arg2;
  var goal = typeof arg1 === 'string' ? arg1 : arg2;

  // check for required args
  if (!days || !goal) {
    console.error('cromedics: last-x-days: invalid args');
  }

  if (!optimizely || !optimizely.get) {
    console.error('cromedics: optimizely.get not defined');
  }

  return !!optimizely.get('behavior').query({
    'version': '0.2',
    'filter': [{ 'field': ['name'], 'value': goal }, { 'field': ['age'], 'comparator': 'lt', 'value': 86400 * 1000 * days }]
  }).length;
}

      },
