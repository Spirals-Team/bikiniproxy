!function(a){"function"==typeof define&&define.amd?define(["jquery"],a):a(jQuery)}(function($){$.fn.tweet=function(a){var b=$.extend({username:null,list:null,query:null,avatar_size:null,count:3,fetch:null,page:1,retweets:!0,intro_text:null,outro_text:null,loading_text:null,refresh_interval:null,twitter_url:"twitter.com",twitter_api_url:"api.twitter.com",twitter_search_url:"search.twitter.com",template:"{avatar}{time}{join}{text}",comparator:function(a,b){return b.cw03_time-a.cw03_time},filter:function(a){return!0}},a),c=/\b((?:[a-z][\w-]+:(?:\/{1,3}|[a-z0-9%])|www\d{0,3}[.]|[a-z0-9.\-]+[.][a-z]{2,4}\/)(?:[^\s()<>]+|\(([^\s()<>]+|(\([^\s()<>]+\)))*\))+(?:\(([^\s()<>]+|(\([^\s()<>]+\)))*\)|[^\s`!()\[\]{};:'".,<>?«»“”‘’]))/gi;function t(a,b){if("string"==typeof a){var c=a;for(var d in b){var e=b[d];c=c.replace(new RegExp("{"+d+"}","g"),null===e?"":e)}return c}return a(b)}$.extend({tweet:{t:t}});function replacer(a,b){return function(){var c=[];this.each(function(){c.push(this.replace(a,b))});return $(c)}}function escapeHTML(a){return a.replace(/</g,"&lt;").replace(/>/g,"^&gt;")}$.fn.extend({linkUser:replacer(/(^|[\W])@(\w+)/gi,'$1<span class="at">@</span><a href="http://'+b.twitter_url+'/$2">$2</a>'),linkHash:replacer(/(?:^| )[\#]+([\w\u00c0-\u00d6\u00d8-\u00f6\u00f8-\u00ff\u0600-\u06ff]+)/gi,' <a href="http://'+b.twitter_search_url+"/search?q=&tag=$1&lang=all"+(b.username&&1==b.username.length&&!b.list?"&from="+b.username.join("%2BOR%2B"):"")+'">#$1</a>')});function linkURLs(a,b){return a.replace(c,function(a){for(var c=/^[a-z]+:/i.test(a)?a:"http://"+a,d=a,e=0;e<b.length;++e){var f=b[e];if(f.url==c&&f.expanded_url){c=f.expanded_url;d=f.display_url;break}}return'<a href="'+escapeHTML(c)+'">'+escapeHTML(d)+"</a>"})}function parse_date(a){return Date.parse(a.replace(/^([a-z]{3})( [a-z]{3} \d\d?)(.*)( \d{4})$/i,"$1,$2$4$3"))}function relative_time(a){var b=arguments.length>1?arguments[1]:new Date,c=parseInt((b.getTime()-a)/1e3,10),d="";d=c<1?"just now":c<60?c+" seconds a