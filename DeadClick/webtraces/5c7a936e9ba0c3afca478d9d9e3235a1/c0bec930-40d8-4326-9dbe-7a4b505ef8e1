jQuery(document).ready(function($){var a="cw18",b={},c=$("."+a),d={width:$("div.cw18").first().width(),height:500},e={next:"Scroll Forward",prev:"Scroll Back",close:"Close"},f;b.GraphicViewer=function(c,$){var f=this,g={},h=[],i=null;f.el=c;f.$el=$(c);var j=f.$el.find("."+a+"graphic"),k=f.$el.find("."+a+"v1");function init(){var a;g.width=j.attr("width");g.height=j.attr("height");k.each(function(){a=new b.PopupWindow(this,g,$);h.push(a)});(g.width>d.width||g.height>d.height)&&(i=new b.GraphicScroller(c,g,$));e.next=f.$el.attr("data-txt-next")?f.$el.attr("data-txt-next"):e.next;e.prev=f.$el.attr("data-txt-prev")?f.$el.attr("data-txt-prev"):e.prev;e.close=f.$el.attr("data-txt-close")?f.$el.attr("data-txt-close"):e.close;bind()}function bind(){k.on("open",popupOpened)}function popupOpened(a,b){a.preventDefault();for(var c=0,d=h.length;c<d;c++)"open"===h[c].state()&&h[c].close();null!=i&&i.showPopup($(b))}init();return{}};b.GraphicScroller=function(b,c,$){var f=this,g,h={distance:5,speed:3,buttonWidth:24,max:null,min:null,orientation:null,cssPoperty:null};f.el=b;f.$el=$(b);var i=f.$el.find("."+a+"content"),j,k;function init(){h.orientation=determineOrientation();h.cssPoperty="horizontal"===h.orientation?"left":"top";f.$el.addClass(a+h.orientation);switch(h.orientation){case"horizontal":createScrollButtons();h.max=-c.width+(d.width-2*h.buttonWidth);h.min=0;break;case"vertical":createScrollButtons();h.max=-c.height+(d.height-2*h.buttonWidth);h.min=0}bind()}function createScrollButtons(){k=$('<a class="'+a+'prev" href="#'+e.prev+'" title="'+e.prev+'"><span>'+e.prev+"</span></a>");j=$('<a class="'+a+'next active" href="#'+e.next+'" title="'+e.next+'"><span>'+e.next+"</span></a>");f.$el.prepend(k);f.$el.append(j)}function bind(){f.$el.on("mousedown touchstart","."+a+"prev.active",scrollPrev_mouseover);f.$el.on("mousedown touchstart","."+a+"next.active",scrollNext_mouseover);f.$el.on("mouseup touchend","."+a+"prev.active",scrollPrev_mouseout);f.$el.on("mouseup touchend","."+a