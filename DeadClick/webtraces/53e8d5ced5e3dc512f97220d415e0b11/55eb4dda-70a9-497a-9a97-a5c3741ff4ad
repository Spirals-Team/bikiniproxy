(function(){function n(t,r){var u=this,s;u.element=typeof t=="object"?t:document.getElementById(t);u.wrapper=u.element.parentNode;u.element.style.webkitTransitionProperty="-webkit-transform";u.element.style.webkitTransitionTimingFunction="cubic-bezier(0,0,0.25,1)";u.element.style.webkitTransitionDuration="0";u.element.style.webkitTransform=l+"0,0"+c;u.options={bounce:e,momentum:e,checkDOMChanges:true,topOnDOMChanges:false,hScrollbar:e,vScrollbar:e,fadeScrollbar:h||!b,shrinkScrollbar:h||!b,desktopCompatibility:false,overflow:"auto",snap:false,bounceLock:false,scrollbarColor:"rgba(0,0,0,0.5)",onScrollEnd:function(){}};if(typeof r=="object"){for(s in r){u.options[s]=r[s]}}if(u.options.desktopCompatibility){u.options.overflow="hidden"}u.onScrollEnd=u.options.onScrollEnd;delete u.options.onScrollEnd;u.wrapper.style.overflow=u.options.overflow;u.refresh();window.addEventListener("onorientationchange" in window?"orientationchange":"resize",u,false);if(b||u.options.desktopCompatibility){u.element.addEventListener(g,u,false);u.element.addEventListener(m,u,false);u.element.addEventListener(f,u,false)}if(u.options.checkDOMChanges){u.element.addEventListener("DOMSubtreeModified",u,false)}}n.prototype={x:0,y:0,enabled:true,handleEvent:function(s){var r=this;switch(s.type){case g:r.touchStart(s);break;case m:r.touchMove(s);break;case f:r.touchEnd(s);break;case"webkitTransitionEnd":r.transitionEnd();break;case"orientationchange":case"resize":r.refresh();break;case"DOMSubtreeModified":r.onDOMModified(s);break}},onDOMModified:function(s){var r=this;if(s.target.parentNode!=r.element){return}setTimeout(function(){r.refresh()},0);if(r.options.topOnDOMChanges&&(r.x!=0||r.y!=0)){r.scrollTo(0,0,"0")}},refresh:function(){var s=this,u=s.x,t=s.y,r;s.scrollWidth=s.wrapper.clientWidth;s.scrollHeight=s.wrapper.clientHeight;s.scrollerWidth=s.element.offsetWidth;s.scrollerHeight=s.element.offsetHeight;s.maxScrollX=s.scrollWidth-s.scrollerWidth;s.maxScrollY=s.scrollHeight-s.scrollerHeight;s.directionX=0;s.directionY=0;if(s.scrollX){if(s.maxScrollX>=0){u=0}else{if(s.x<s.maxScrollX){u=s.maxScrollX}}}if(s.scrollY){if(s.maxScrollY>=0){t=0}else{if(s.y<s.maxScrollY){t=s.maxScrollY}}}if(s.options.snap){s.maxPageX=-Math.floor(s.maxScrollX/s.scrollWidth);s.maxPageY=-Math.floor(s.maxScrollY/s.scrollHeight);r=s.snap(u,t);u=r.x;t=r.y}if(u!=s.x||t!=s.y){s.setTransitionTime("0");s.setPosition(u,t,true)}s.scrollX=s.scrollerWidth>s.scrollWidth;s.scrollY=!s.options.bounceLock&&!s.scrollX||s.scrollerHeight>s.scrollHeight;if(s.options.hScrollbar&&s.scrollX){s.scrollBarX=s.scrollBarX||new o("horizontal",s.wrapper,s.options.fadeScrollbar,s.options.shrinkScrollbar,s.options.scrollbarColor);s.scrollBarX.init(s.scrollWidth,s.scrollerWidth)}else{if(s.scrollBarX){s.scrollBarX=s.scrollBarX.remove()}}if(s.options.vScrollbar&&s.scrollY&&s.scrollerHeight>s.scrollHeight){s.scrollBarY=s.scrollBarY||new o("vertical",s.wrapper,s.options.fadeScrollbar,s.options.shrinkScrollbar,s.options.scrollbarColor);s.scrollBarY.init(s.scrollHeight,s.scrollerHeight)}else{if(s.scrollBarY){s.scrollBarY=s.scrollBarY.remove()}}},setPosition:function(r,u,t){var s=this;s.x=r;s.y=u;s.element.style.webkitTransform=l+s.x+"px,"+s.y+"px"+c;if(!t){if(s.scrollBarX){s.scrollBarX.setPosition(s.x)}if(s.scrollBarY){s.scrollBarY.setPosition(s.y)}}},setTransitionTime:function(s){var r=this;s=s||"0";r.element.style.webkitTransitionDuration=s;if(r.scrollBarX){r.scrollBarX.bar.style.webkitTransitionDuration=s;r.scrollBarX.wrapper.style.webkitTransitionDuration=e&&r.options.fadeScrollbar?"300ms":"0"}if(r.scrollBarY){r.scrollBarY.bar.style.webkitTransitionDuration=s;r.scrollBarY.wrapper.style.webkitTransitionDuration=e&&r.options.fadeScrollbar?"300ms":"0"}},touchStart:function(t){var s=this,r;if(!s.enabled){return}t.preventDefault();t.stopPropagation();s.scrolling=true;s.moved=false;s.distX=0;s.distY=0;s.setTransitionTime("0");if(s.options.momentum||s.options.snap){r=new WebKitCSSMatrix(window.getComputedStyle(s.element).webkitTr