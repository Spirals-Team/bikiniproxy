        url: 'https://queso-cdn.prod.reuters.tv/index.html',
        resizable: false,
        mid: '127339910',
        title: RTR_VideoTitle,
        hide_title: true,
        poster: 'https://s1.reutersmedia.net/resources/r/?d=20170328&i=127339910&w=652&r=127339910-1&t=2',
        suppress_ads: false,
        suppress_rtv: true,
        muted:  false,
        autoplay: true,
        preroll: true,
        allow_collapse: false,
        draggable: false,
        chartbeat_uid: 52639,
        ad_url: adOpVar
    });

    window.RTVEmbedder.addEventListener('ended', goToNextVideo);
    function goToNextVideo(type, params) {
        if (params.ima_playing !== 'true'){
            setTimeout(function(){
                location.replace('/video/2018/02/15/ex-student-kills-17-in-shooting-spree-at?videoId=401308940&videoChannel=1&channelName=Top+News');
            }, 500);
        }
    }

});
var RCOM_map = {
    first_quartile: 'video_player.story.quartile25',
    second_quartile: 'video_player.story.quartile50',
    third_quartile: 'video_player.story.quartile75',
    fourth_quartile: 'video_player.story.quartile100',
    pause: 'video_player.story.pause',
    resume: 'video_player.story.resume',
    seek: 'video_player.story.scrub',
    skip: 'video_player.story.skip',
    mute: 'video_player.story.mute',
    unmute: 'video_player.story.unmute',
    fullscreen : 'video_player.story.fullscreen',
    click_out : 'video_player.story.click_out',
    preroll_start : 'video_player.ad.started',
    preroll_end : 'video_player.ad.finished',
    ad_skip : 'video_player.ad.skip',
    opener_start : 'video_player.opener.started',
    opener_end : 'video_player.opener.finished',
    start: 'video_player.session.started',
    ended: 'video_player.session.finished',
    story_start: 'video_player.story.started',
    story_end: 'video_player.story.finished',
};

var trackEventHandler = function(type, params) {
    videoQuartile = '';
    if (RCOM_map[type] == 'video_player.session.started'){
        videoType = 'START';
    } else if (RCOM_map[type] == 'video_player.session.finished'){
        videoType = 'CLOSER';
        videoQuartile = '1';
    } else if (RCOM_map[type] == 'video_player.opener.started' || RCOM_map[type] == 'video_player.opener.finished'){
        videoType = 'OPENER';
    } else if (RCOM_map[type] == 'video_player.story.quartile25'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.25';
    } else if (RCOM_map[type] == 'video_player.story.quartile50'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.50';
    } else if (RCOM_map[type] == 'video_player.story.quartile75'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.75';
    } else if (RCOM_map[type] == 'video_player.story.quartile100'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '1';
    }  else if (RCOM_map[type] == 'video_player.ad.started' || RCOM_map[type] == 'video_player.ad.finished' || RCOM_map[type] == 'video_player.ad.skip'){
       videoType = 'PREROLL';
    }  else{ videoType = 'STORY'; }

    if (abp == false || abp == '0'){ adBlocked = '0'; } else { adBlocked = '1'; }

    

    if (RCOM_map[type] == 'video_player.session.finished'){
        analytics.track('video_player.story.quartile', {
            video_player_video_completion_pct: '1',
            video_player_video_type: 'STORY',
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
        analytics.track('video_player.story.finished', {
            video_player_video_completion_pct: videoQuartile,
            video_player_completion_secs: Reuters.inlinevideo.lengthseconds,
            video_player_video_type: 'STORY',
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
        analytics.track('video_player.session.finished', {
            video_player_video_completion_pct: videoQuartile,
            video_player_video_type: 'CLOSER',
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
    } else if (RCOM_map[type] == 'video_player.story.finished' ){
        analytics.track('video_player.story.finished', {
            video_player_video_completion_pct: videoQuartile,
            video_player_video_type: 'STORY',
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
    } else if (RCOM_map[type] == 'video_player.session.started'){
        analytics.track('video_player.session.started', {
            video_player_video_completion_pct: videoQuartile,
            video_player_video_type: videoType,
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
    } else if (videoQuartile != '' && (RCOM_map[type] != 'video_player.session.finished' || RCOM_map[type] != 'video_player.story.finished' ) && (RCOM_map[type] != 'video_player.session.started' || RCOM_map[type] != 'video_player.ad.started' )){
        analytics.track(RCOM_map[type], {
            video_player_video_completion_pct: videoQuartile,
            video_player_video_type: videoType,
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        });
    } else{
        analytics.track(RCOM_map[type], {
            video_player_video_type: videoType,
            topic_channel: document.querySelector("meta[name='analyticsAttributes.topicChannel']").content,
        topic_sub_channel: document.querySelector("meta[name='analyticsAttributes.topicSubChannel']").content,
        content_type: document.querySelector("meta[name='analyticsAttributes.contentType']").content,
        edition: document.querySelector("meta[name='analyticsAttributes.edition']").content,
        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
        video_player_video_category: Reuters.inlinevideo.channelName,
        video_player_video_source: Reuters.inlinevideo.source,
        video_player_video_length: Reuters.inlinevideo.lengthseconds,
        video_player_video_title: Reuters.inlinevideo.title,
        video_player_video_asset_id: Reuters.inlinevideo.id
        })
    }

}

var trackEventTypes = ['start', 'story_start', 'story_end', 'ad_start', 'live_replay_start', 'live_replay_end', 'live_now_start', 'live_now_end', 'ad_end', 'opener_start', 'opener_end', 'clip_start', 'clip_end', 'closer_start', 'closer_end', 'first_quartile', 'second_quartile', 'third_quartile', 'fourth_quartile', 'complete', 'ad_skip', 'pause', 'resume', 'mute', 'unmute', 'click_out', 'skip', 'seek', 'minimize', 'dismiss', 'ended', 'fullscreen', 'preroll_end'];

for(var i in trackEventTypes) {
  var ev = trackEventTypes[i];
  window.RTVEmbedder.addEventListener(ev, trackEventHandler);
}


</script>

</div>

