	html#ttcobrand .module .list-post-item:hover .list-post-link,
	html#ttcobrand .thumbnails-a .videoCube:hover .video-label-box .video-title {
		text-decoration: underline !important;
	    border-bottom: none;
	    color: #06f;
	    display: initial;
	}
	html#ttcobrand .module.mod-type-taboola {
		display: block !important;
	}
	</style>
</section>	    				    	    				        

                
	




	    				    	    														</section>

