function dontGoOffScreenY(switchTo,resetTo){if(((offsetTop-$(window).scrollTop()-tooltipHeight-offsetY-12)<0)&&(resetTo.indexOf('top')>-1)){object.options.position=switchTo;resetPosition=resetTo;}
