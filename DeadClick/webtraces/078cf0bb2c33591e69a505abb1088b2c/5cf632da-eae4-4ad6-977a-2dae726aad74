    var TeadsApiRPC = (function () {
        function TeadsApiRPC() {
            var _this = this;
            this.runningRPCs = [];
            this.rpcCallback = function (evt) {
                if (typeof evt.data === 'object') {
                    var msg = evt.data;
                    if (!msg.method)
                        return;
                    switch (msg.method) {
                        case TeadsApiRPC.MSG_HELLO:
                            if (_this.pairedAdWindow)
                                throw new Error('TeadsApiRPC is already paired');
                            _this.pairedAdWindow = evt.source;
                            var match = _this.runningRPCs.splice(0, 1)[0];
                            clearTimeout(_this.searchTimeoutInterval);
                            match.promise.succeed(_this.pairedAdWindow);
                            break;
                        default:
                            var firstRPCMatchIndex = -1;
                            try {
                                while (_this.runningRPCs[++firstRPCMatchIndex].rpcData.method != msg.method) { }
                                var match_1 = _this.runningRPCs.splice(firstRPCMatchIndex, 1)[0];
                                match_1 && match_1.promise.succeed(msg.methodReturn || msg.error);
                            }
                            catch (er) {
                                bnt.logger.warn("Mathing rpc not found - \"" + msg.method + "\"");
                            }
                            break;
                    }
                }
                if (!_this.runningRPCs.length)
                    window.removeEventListener('message', _this.rpcCallback);
            };
        }
        TeadsApiRPC.prototype.pairWithPlayer = function () {
            if (this.pairedAdWindow)
                throw new Error('TeadsApiRPC is already paired');
            window.addEventListener('message', this.rpcCallback);
            var wParent = window;
            while ((wParent != wParent.parent) && (wParent = wParent.parent)) {
                wParent.postMessage({ method: TeadsApiRPC.MSG_HELLO }, '*');
            }
            var pr = new bnt.Promise();
            this.runningRPCs.push({ rpcData: { method: TeadsApiRPC.MSG_HELLO }, promise: pr });
            this.searchTimeoutInterval = setTimeout(function () {
                pr.fail(new Error('Timeout'));
                bnt.logger.warn('TeadsApi not found in set time interval');
            }, TeadsApiRPC.TIMEOUT);
            return pr.future;
        };
        TeadsApiRPC.prototype.sendMessage = function (message) {
            window.addEventListener('message', this.rpcCallback);
            var pr = new bnt.Promise();
            this.runningRPCs.push({ rpcData: message, promise: pr });
            this.pairedAdWindow.postMessage(message, '*');
            return pr.future;
        };
        TeadsApiRPC.prototype.performTapMode = function (on) {
