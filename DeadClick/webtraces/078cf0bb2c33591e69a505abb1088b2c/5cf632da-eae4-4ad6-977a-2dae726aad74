            if (this.getTimeRemaining() === 0) {
                this.stop();
            }
            _super.prototype.play.call(this);
        };
        Timer.prototype.getTimeRemaining = function () {
            return Math.max(this.duration - this.getTime(), 0);
        };
        return Timer;
    }(Stopwatch));
    bnt.Timer = Timer;
