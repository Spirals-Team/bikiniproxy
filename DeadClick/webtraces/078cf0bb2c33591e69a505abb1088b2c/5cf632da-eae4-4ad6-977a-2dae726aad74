                var arr = [];
                container.forEachEos(function (eos) {
                    if (eos.element.hasOwnProperty("timeline"))
                        arr.push(eos.element);
                    if (eos.element instanceof bnt.Group) {
                        arr = arr.concat(fn(eos.element));
                    }
                });
                return arr;
            };
