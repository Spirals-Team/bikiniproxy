        configure: function () {},
        write: function () {},
        show: function () {},
        isActive: function () {
            return false;
        },
        setScriptRoot: function () {},
        setImgRoot: function () {},
        getAdvertTag: function () {},
        getSectionPath: function() {}
    };
}
