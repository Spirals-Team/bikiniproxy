	'use strict';
	var normUserAgent = navigator.userAgent.toLowerCase();
	
	/******************above or below video player**********************/
	/*finds Y position of given obj*/
	function findPosY(obj)
	{
		var top = 0;
		if(obj.offsetParent)
		{
			while(1)
			{
			  top += obj.offsetTop;
			  if(!obj.offsetParent)
				break;
			  obj = obj.offsetParent;
			}
		}
		else if(obj.y)
		{
			top += obj.y;
		}
		return top;
	}

	var taboola = window.document.getElementById('taboola-below-main-column');
	var ooyalaplayer = window.document.getElementById('ooyalaplayer');
	var ooyalaplayer_wide = window.document.getElementById('ooyalaplayer-wide');
	var taboola_offsetTop = 0;
	var ooyalaplayer_offsetTop = 0;
	var ooyalaplayer_wide_offsetTop = 0;
	var suffix = '';

	/*checking if searched elements are defined*/
	if(taboola) { taboola_offsetTop = findPosY(taboola); }
	if(ooyalaplayer) { ooyalaplayer_offsetTop = findPosY(ooyalaplayer); }
	if(ooyalaplayer_wide) { ooyalaplayer_wide_offsetTop = findPosY(ooyalaplayer_wide); }

	/*actually checking if thumbs-1r above or below video player*/
	if(taboola_offsetTop != 0) {
		if(ooyalaplayer_offsetTop != 0) {
			if(taboola_offsetTop > ooyalaplayer_offsetTop)
				suffix = '-below-video';
			else
				suffix = '-above-video';
		} else if(ooyalaplayer_wide_offsetTop != 0) {
			if(taboola_offsetTop > ooyalaplayer_wide_offsetTop)
				suffix = '-below-video';
			else
				suffix = '-above-video';
		}
	}
	/**************************************************/
	
	if (typeof req.uip !== 'undefined' && normUserAgent.match(/(iPhone|iPod|Android.*Mobile|IEMobile.*|BlackBerry|BB10|Opera\sMini)/i)){
		req.uip += '-mobile';
	} else if (typeof req.uip !== 'undefined'){
		req.uip;
	}
	
	/* set url of the request to be the value in the og:url meta tag */ 
	var metaTags = document.getElementsByTagName('meta');
	var newUrl = undefined;
	for (var i=0; i<metaTags.length; i++){
		if (typeof metaTags[i].getAttribute('property') !== 'undefined' && metaTags[i].getAttribute('property') === 'og:url'){
			newUrl = metaTags[i].getAttribute('content');
			break;
		}
	}
	if (typeof newUrl !== 'undefined' && newUrl.match('.*businessinsider\.com.*')){
		if (typeof req.u !== 'undefined' && typeof req.u == 'string'){		
			req.u = newUrl;
		}
		if (typeof req.ii !== 'undefined' && typeof req.ii == 'string'){
			var newItemID = newUrl.replace(/^(?:\/\/|[^\/]+)*/, '');
			req.ii = newItemID;
		}
	}
	if (mode !== null && window.location.href.indexOf('www')>-1 && window.location.href.indexOf('www')<=8) {
		req.ac='www';
	}
	if (mode !== null && window.location.href.indexOf('uk')>-1 && window.location.href.indexOf('www')<=8){
		req.ac='uk';
	}
	if (window.location.href.indexOf('uk')>-1 && window.location.href.indexOf('www')<=8 && req 
		&& typeof req.u !== 'undefined' && typeof req.u == 'string'
	 	&& req.u !== 'undefined' && typeof req.u == 'string'){
		req.ii=req.u;
	}
	return req;
}
