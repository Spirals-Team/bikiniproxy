                                        currentModeContainerParent.insertBefore(currentModeContainer, containerToMoveAbove);
                                }
                        }
                        else if (this.mode_name === 'ab_thumbs-2r-mobile_above-native' || this.mode_name === 'ab_thumbs-2r-mobile_plus-1-row') {

                                var currentModeContainer = this.container,
                                        currentModeContainerParent = currentModeContainer.parentNode,
