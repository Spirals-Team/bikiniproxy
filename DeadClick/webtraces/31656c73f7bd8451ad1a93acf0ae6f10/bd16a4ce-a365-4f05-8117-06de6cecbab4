      if (!_settings.shownOnce) {
        _settings.shownOnce = true;
        showPopup();
      }
    });
  
  }
  
  function showPopup() {
    $('.optly-modal').css("display", "block");
  }

  function hidePopup() {
    $('.optly-modal').css("display", "none");
    window['optimizely'] = window['optimizely'] || [];
