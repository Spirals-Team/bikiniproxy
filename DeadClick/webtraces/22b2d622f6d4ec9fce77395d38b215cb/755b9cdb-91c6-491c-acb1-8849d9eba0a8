MenuManager.prototype.dropHandler=function(a){if(null!=a.currentElementOver&&null!=a.currentElementOver.$li&&a.currentElementOver.$li.hasClass("liDropabble")&&!a.currentElementOver.$li.hasClass("beingDragged")){var b=a.currentElementOver.$li;b.removeClass("hover");if(this.sideMenuHasCustomLinks||a.$draggedElement.data("drag").menuType===b.data("drag").menuType){var c="",d=!1;a.$draggedElement.data("drag").isAddPage?(d=!0,c="/Pages/Page/Add"):c="/Pages/PageStatus/Move/"+a.$draggedElement.data("drag").itemID;
