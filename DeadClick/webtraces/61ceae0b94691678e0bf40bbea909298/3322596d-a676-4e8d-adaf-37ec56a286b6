<script language="JavaScript">
function isNothing(x) {
  if ( typeof(x) == "undefined" || x == null || x == "" || x == "undefined" ) {
	alert("Please enter a vitamin or supplement name or a medical condition.");
	document.getElementById('vitsearchfield').focus();
	return true;
	}
  else {
    return false;
  }
}
</script>

