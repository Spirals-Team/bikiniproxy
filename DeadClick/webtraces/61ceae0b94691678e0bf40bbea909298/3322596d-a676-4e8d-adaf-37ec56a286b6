<script type="text/javascript">

		function highlightNav(divName,leftBinChapter) {

			if (!document.getElementsByTagName) return false;
			if (!document.getElementById) return false;
			if (!document.getElementById(divName)) return false;

			var nav = document.getElementById(divName);
			var links = nav.getElementsByTagName("a");

			for (var i=0; i<links.length; i++) {
				var linkurl = links[i].getAttribute("href");
				linkurl = linkurl.toLowerCase();
				if (linkurl.lastIndexOf("?") != -1) {
					linkurl = linkurl.substring(0,linkurl.lastIndexOf("?"));
				}
				if (linkurl.lastIndexOf("#") != -1) {
					linkurl = linkurl.substring(0,linkurl.lastIndexOf("#"));
				}
				if (rtrimNumbers(linkurl) == rtrimNumbers(leftBinChapter)) {
					addClassName(links[i].parentNode, 'here');
				}
			}
		}

		function rtrimNumbers(str) {
			var str_trimmed = [];
			var stop_trimming = false;
			var currentChar;
			for (var i=str.length; i>0; i--) {
				currentChar = str.substring((i-1),i);
				if (i == str.length && isNaN(currentChar)) return str;
				if (isNaN(currentChar)) stop_trimming = true;
				if (isNaN(currentChar) || stop_trimming) str_trimmed.push(currentChar);
			}
			str_trimmed = str_trimmed.reverse();
			return String(str_trimmed.join(''));
		}

		var theUrl=location.href;
		var s_pagenameCheck = theUrl.toLowerCase();

		if (s_pagenameCheck.lastIndexOf("?") != -1) {
			var s_pagenameCheck = s_pagenameCheck.substring(0,s_pagenameCheck.lastIndexOf("?"));
		}

		highlightNav("st-sbnav",s_pn);

		</script><div id="standardSubnav_rdr" class="standardSubnav_rdr_fmt">
