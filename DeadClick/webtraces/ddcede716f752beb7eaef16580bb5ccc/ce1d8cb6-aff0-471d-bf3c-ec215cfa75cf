(function(e,t,n,r){function i(t,n){this.element=t;var r=this,i=e(r.element),s=t.id,o=t.className.split(" "),o=o[0],u;s?u=s:u=o;i.wrap('<div class="allRas" id="AllRAS-'+u+'"/>');r.init(n,u);e.fn.ras.slide=function(e,t,n){r.slide(e,t,n)}}e.ras=e.ras||{};e.ras.slideshow={options:{duration:400,textPrev:"prev",textNext:"next"}};i.prototype={init:function(n,r){var i=this,s=e(i.element),o=s.children(),u='<div id="navRAS"><button class="btnSlideRAS prev">'+n.textPrev+'</button><button class="btnSlideRAS next">'+n.textNext+"</button></div>",a=o.length,f=s.parent();s.wrap('<div class="rasContainer" id="RASContainer-'+r+'" style="width:100%; overflow:hidden;"/>'),$RASContainer=s.parent();if(a>1){$RASContainer.before(u);var l=$RASContainer.prev("#navRAS"),c=l.next().children(),h=e(i.element),o=h.children(),p=h.parent(),d=e(t).width();o.css({"float":"left",position:"relative"}).each(function(t){e(this).attr("data-slide",t)});var v=f.width(),m=v;h.css({width:m*a}).attr("data-width",m);e("img").load(function(){i.heightElement(o,h);h.css({width:v*a}).attr("data-width",v);o.css({width:v,position:"relative"})});l.find(".prev").bind("click",function(){var e=n.duration,t=n.animation;i.slide(c,"prev",e,"null")});l.find(".next").bind("click",function(){var e=n.duration,t=n.animation;i.slide(c,"next",e,"null")});e(t).resize(function(){var e=p.width();i.resizeMe(h,e,a)})}},slide:function(t,n,r){var i=t.data("width"),s=t.width(),o=t.children(":first"),u=t.children(":last");if(n=="prev"){t.children().css({"margin-left":0});u.prependTo(t).css({"margin-left":"-"+i+"px"}).animate({"margin-left":0},r)}else if(n=="next"){o.animate({"margin-left":"-"+i+"px"},r,function(){e(this).css({"margin-left":0}).appendTo(t)})}},heightElement:function(t,n){var r=0;t.each(function(){var t=e(this).height();if(t>r){r=t}});n.height(r);n.parent().height(r).attr("data-height",r)},resizeMe:function(e,t,n){var r=e.children();var i=t,s=i;e.css({width:t*n}).attr("data-width",t);this.heightElement(r,e)}};e.fn.ras=function(t){t=e.extend({},e.ras.slideshow.options,t);return this.each(function(){new i(this,t)})}})(jQuery,window,document);
(function(e,t,n,r){var i=e.ras.slideshow;i.autoslide={options:{time:2e3,textPlay:"play",textPause:"pause",duration:700}};e.fn.autoslide=function(t){t=e.extend({},i.autoslide.options,t);return this.each(function(){var n=e(this),r=e(this).parent().parent(),i='<button id="playRas">'+t.textPlay+'</button><button id="stopRas">'+t.textPause+"</button>",s=r.find("#navRAS");s.append(i);var o=s.find("#playRas"),u=s.find("#stopRas");stopFct=function(){autoSlideShow=clearInterval(autoSlideShow)};playFct=function(){autoSlideShow=setInterval(function(){e.fn.ras.slide(n,"next",t.duration)},t.time)};playFct();o.bind("click",function(){playFct()});u.bind("click",function(){stopFct()})})}})(jQuery,window,document);
/* [URL] */
;(function($, window, document, undefined) {

  var r = $.ras.slideshow;
  
  r.carousel = {
    options : {
      number:3,
      oneVisible:false
    }
  };
  
  $.fn.carousel = function(options) {
    
    options = $.extend({}, r.carousel.options, options);
    return this.each(function(){
      var slideshow = $(this),
          countElement = options.number,
          childs = slideshow.children(),
          docWidth = $(this).parent().width(),
          childsLength = childs.length,
          oneVisible = options.oneVisible,
          elementRAS = $(self.element);
          
      
      if(childsLength > countElement && !oneVisible){
        for(var i = 0; i < childs.length; i=i+countElement) {
          childs
            .slice(i, i+countElement)
            .wrapAll('<div class="slide" style="float:left; width:'+docWidth+'px;"/>');
        }
        //If oneVisible is false  
        $('img').load(function(){
          childs.css({
            'width':100/countElement + '%'
          });
        });
        
      }else{
        var widthElement;
        ($(window).width() > 400 ? widthElement = docWidth/countElement : widthElement = $(window).width());
          
        $('img').load(function(){
          childs
            .css({
              'width':widthElement + 'px',
              'float':'none',
              'display':'inline-block',
              'white-space':'normal',
              'vertical-align':'top'
            });
          slideshow.css({
            'width':'100%', 
            'white-space':'nowrap'
          }).attr('data-width',widthElement);
        });
        
      }
      
      $(window).resize(function(){
        var valueScreen = slideshow.closest('.allRas').width();
        
        function changeValue(value){
          childs.css({
            'width':value,
            'position':'relative'
          });
        }
        if(oneVisible){
          changeValue(widthElement);
          slideshow.attr('data-width',widthElement);
          if(valueScreen < widthElement) {
            changeValue(valueScreen);
            slideshow.attr('data-width',valueScreen);
          }
        }/*else {
          changeValue(docWidth);
        }*/
      });
      
    });
    
  };
  
})(jQuery, window, document);;
(function ($) {

  /*========
  
   = Init
  
  ========*/

  Drupal.angers = Drupal.angers || {}
  Drupal.settings.angers = Drupal.settings.angers || {}

  // true if we are in touch screen
  var isTouch = ('ontouchstart' in window) ||
                window.DocumentTouch && document instanceof DocumentTouch;


  /** ********************************************************************
  
   * FUNCTIONS
   
   ** ***************************************************************** */
  
  /*========
  
   = Enlarge Your Click Zone : allow link'parent element to be clickable too
  
  ========*/
  Drupal.angers.enlargeYourClick = function(selector){
    $(selector).once('enlargeYourClick').click(function(e){
      // don't handle if user click on a link, or if he click with mouse wheel
      if (e.target.tagName != "A" && e.button != 1) {
        var dest = $(this).find('a:first').not('.contextual-links-trigger').attr('href');
        if (dest) {window.location = dest};
      }
    })
    .css({cursor: 'pointer'});
  } // Drupal.basement.enlargeYourClick
  
  
  /*========
  
   = Primary Menu
  
  ========*/
  Drupal.angers.primarymenu = function(selector){
    $(selector).once('primarymenu', function(){
      var thisli = $(this);

      var ulElement = $(this).find('> ul'),
          liElement = ulElement.find('> li');
          
      nbreElement = liElement.length;
      if(nbreElement > 4){
        for(var i = 0; i < nbreElement; i+=4) {
          liElement
            .slice(i, i+4)
            .wrapAll('<div class="groupLi"/>');
        }
      }
      if($('html').hasClass('js')){
        ulElement.hide();
      }

      // Gestion des touchscreen, au premier clic, il ne faut pas aller sur le lien
      if(isTouch) {
        $(this).find('> a').click(
          function() {
            if($(this).hasClass('touch-click') && thisli.find('> ul').is(':visible')) {
              $(this).removeClass('touch-click');
            }
            else {
              thisli.parent().find('.touch-click').removeClass('touch-click');
              $(this).addClass('touch-click');
              return false;
            }
          }
        )
      }

      $(this).hover(
        function() {
          ulElement.stop(1,1).delay(250, function() {
            $(this).stop(1,1).fadeIn(300)
          })
        },
        function() {
          ulElement.stop(1,1).delay(100, function() {
            $(this).stop(1,1).fadeOut(100)
          })         
        }
      );
    });
  }
  
  /*========
  
   = Sidebar Tabs
  
  ========*/
  Drupal.angers.sidebarTabs = function(selector){
    $(selector).once('sidebarTabs', function(){
      $(this).find('li').each(function(){
        var aText = $(this).find('a').text();
        $(this).find('a').text('');
        var popIn = '<div class="popIn">'+aText+'</div>';
        $(this).append(popIn);
      });
    });
  }
  
  /*========
  
   = Page Rubriquage
  
  ========*/
  Drupal.angers.slideRubriquage = function(selector){
    $(selector).once('SlideRubriquage', function(){
      var slideShow = $(this);
      slideShow
        .before('<div id="nav"/>')
        .cycle({
          timeout:10000,
          pager:"#nav"
        });
    });
  }

  /*========
  
   = Carousel prestataire
  
  ========*/
  Drupal.angers.carouselPrestataire = function(selector){
    $(selector).once('carouselPrestataire', function () {
      var navCarousel = $(this).find('.lcarrousel-thumbs'),
          bigCarousel = $(this).find('.lcarrousel-big');
      
      navCarousel
        .find('li:first')
        .addClass('active');
        
      navCarousel
        .find('li')
        .each(function(e){
          $(this).click(function(){
            navCarousel
              .find('li')
              .removeClass('active');
            var element = bigCarousel.find('li:eq('+e+')'),
                allElement = bigCarousel.find('li');
            if(element.is(':hidden')){
              allElement.hide();
              element.show();
              $(this).addClass('active');
            }
          });
        });
    });
  };
  
  /*========
  
   = Agenda Home
  
  ========*/
  
  Drupal.angers.AgendaHome = function(selector){
    $(selector).once('AgendaHome', function () {
      var NBBYSLIDE = 3;
      var tabContainer = $(this),
          tabTitle = tabContainer.find('.tab-title'),
          tabContent = tabContainer.find('.tab-content'),
          navSlide = '<div class="nav"><a class="slideButton prev"></a> <a class="slideButton next"></a></div>';
          
      tabContent
        .each(function(){
        if($(this).children().length > NBBYSLIDE){
          $(this).wrap('<div class="scrollable"/>');

          var nbSlides = Math.ceil($(this).children().length/NBBYSLIDE);
          for(var i = 0; i < nbSlides; i++) {
            $(this)
              .children()
              .slice(i, i+NBBYSLIDE)
              .wrapAll('<div class="slide"/>');
          }
        }else{
          $(this).addClass('alone');
        }
      });
      
      var scrollable = $('.scrollable'),
          alone = $('.alone');
      
      scrollable.hide();
      alone.hide();
      
      /* Show first element*/
        
      tabContainer
       .find('.item-0 .scrollable, .item-0 .alone')
       .show();
       
      tabContainer
       .find('.item-0 .tab-title')
       .addClass('active');
       
      /* Title event : Gestion des onglets */
      
      tabTitle
        .before(navSlide)
        .each(function(){
          var nav = $('.nav');
          nav.hide();
          
          // Show first Nav
          tabContainer
            .find('.item-0 .nav')
              .show();
          
          //if alone hide first nav  
          tabContainer
            .find('.alone')
            .prevAll('.nav')
              .hide();
          
          $(this).click(function(){
            tabTitle.removeClass('active');
            $(this).addClass('active');
            var next = $(this).next(),
                prev = $(this).prev();
            
            tabTitle.next().hide();
            nav.hide();
            
            if(next.is(':hidden')){
              var h = 0;
              next.show();
              if(next.is(':not(.alone)')){
                next.find('.slide').each(function(){
                  var hTallest = $(this).height();
                  if(hTallest > h){
                    h = hTallest;
                  }
                });

                next.css({'height': h/13 + 2 +'em'});
              }
              if(prev.hasClass('nav') && next.hasClass('alone')){
                prev.hide();
              }else{
                prev.show();
              }
            }
          });  
      });
      
      
      
      /*Scrollable Function*/
      scrollable.each(function(){
        var slideshow = $(this);
        var h = 0;
        
        $('img').each(function(){
          var srcImg = $(this).attr('src');
          $(this).load(function(){
            slideshow.find('.slide').each(function(){
              var hTallest = $(this).height();
              if(hTallest > h){
                h = hTallest;
              }
            });
            //Attribute heigth element and Container
            slideshow.css({'height': h/13 + 2 +'em'});
          }).attr('src',srcImg);
        });
        
        
        slideshow.scrollable({
          circular:true,
          onBeforeSeek: function(event, target) {
            //ajust scrollable en fonction de la taille des items du slide
            var item = slideshow.find(".slide").next().eq(target);
              slideHeight = (item.height()/13);
              slideshow.animate({'height':slideHeight + 2 +'em'});
          }
        });
      });

      /* Add Google event tracking on prev / next click */
      // Mimic a slide pagination to store in google tracking
      if (typeof(_gaq) == 'object') {
        $(this).find('.angerslt-tabs-tab-item').each(function(slideIndex, Element) {
          var nbSlides = $(this).find('.slide').length;
          var slidePagination = 1;

          var category = 'Carousel';
          var opt_label = 'Slide Agenda Home Tab ' + slideIndex;

          $(this).find('.prev').click(function() {
            slidePagination == 1 ? slidePagination = nbSlides : slidePagination--;
            _gaq.push(['_trackEvent', category, opt_label, "Slide " + slidePagination]);
          });
          $(this).find('.next').click(function() {
            slidePagination == nbSlides ? slidePagination = 1 : slidePagination++;
            _gaq.push(['_trackEvent', category, opt_label, "Slide " + slidePagination]);
          });
        });
      }

    });
  };
  
  /*========
  
   = Ubox SlideShow
  
  ========*/
  
  Drupal.angers.UboxSlideshow = function(selector){
    $(selector).once('UboxSlideshow', function () {
      var navSlide = '<a class="slideButton prev"></a> <a class="slideButton next"></a>',
          slideshow = $(this),
          uboxContent = $(this).find('.ubox-content'),
          countElement = slideshow.data('ubox-count');
          
      slideshow.each(function(){
        uboxContent.wrap('<div id="scrollable"/>');
        var $scrollable = $(this).find('#scrollable');
      
        //pour placer les boutons du slide
      
        $scrollable.wrap('<div class="containerScrollable"/>');
        $('.containerScrollable').prepend(navSlide);
        
        var widthScrollable = Math.round($scrollable.width());
        /*Bug with Scrollable and padding element in scrollable*/
        $('#scrollable .ubox-content').css({
          "margin-left":"0.4em"
        });
        /*Function Group Element pour grouper des elements par n*/
        var nbreElement = slideshow.data('ubox-total');
        for(var i = 0; i < nbreElement; i++) {
          uboxContent
            .find('> div')
            .slice(i, i+countElement)
            .wrapAll('<div class="slide" style="width:'+widthScrollable+'px;"/>');
        }
        //initialize la hauteur de scrollable
        
        $('img').load(function(){
          var heightScrollable = Math.round(uboxContent.height()/13);
          $scrollable.css({'height':heightScrollable + 'em'});
        })
  
        
        //Scrollable function
        $scrollable.scrollable({
          circular:true,
          onBeforeSeek: function(event, target) {
            $('#scrollable .ubox-content').css({
              "margin-left":"0.4em"
            });
            //ajust scrollable en fonction de la taille des items du slide
            var item = $scrollable.find(".slide").next().eq(target);
              slideHeight = Math.round(item.height()/13);
              $scrollable.animate({'height':slideHeight + 'em'});
          }
        });
      });
    });
  };
  
  /*========
  
   = SlideShow OT
  
  ========*/

  Drupal.angers.SlideContactOT = function(selector){
    $(selector).once('SlideContactOT', function (e) {
      var slideContainer = $(this).find('.contact_forms_list_inb_container'),
          navSlide = "<div class='navSlideContact' id='navSlideContact"+e+"'/>";

        slideContainer
          .before(navSlide)
          .cycle({
            timeout: 5000,
            pager:'#navSlideContact'+e+''
          });
    });
  };


  /*========

   = SlideShow Events

   ========*/

  Drupal.angers.SlideEvents = function(selector){
    $(selector).once('SlideEvents', function (e) {
      var slideContainer = $(this).find('> .content'),
        navSlide = "<div class='navSlideEvent' id='navSlideEvent"+e+"'/>";

      slideContainer
        .before(navSlide)
        .cycle({
          timeout: 3000,
          pager:'#navSlideEvent'+e+''
        });
    });
  };

  /*========

   = SlideShow Events

   ========*/

  Drupal.angers.TabsEventsHome = function(selector){


   var i = 0;
    var content = $(selector).find('.ctnt');
    $('.tab').each(function(){

      // 1er tab actif
      if (i > 0) { $(this).find('.ctnt').addClass('hide'); }
      else { $(this).find('.thumb').addClass('active'); } i++;

      var title = $(this).find('.thumb');
      title.click(function(){
        $(selector).find('.thumb').removeClass('active');
        $(this).addClass('active');
        content.addClass('hide');
        $(this).next().removeClass('hide');
      });
    })
  };


  /*========

   = Tabs Filter Map

   ========*/

  Drupal.angers.TabsFilterMap = function(selector){


    $(selector).find('fieldset').each(function(){
      var tab = $(this);
      tab.find('legend').click(function (){
        $(this).next().toggle();
        tab.toggleClass('open');
      });
    })
  };


  /*========
  
   = Tabs Comment and Vote
  
  ========*/
  
  Drupal.angers.TabsCommentVote = function(selector){
  	$(selector).once('TabsCommentVote', function(){
  		var title = $(this).find('> h3'),
  		    tabs = $(this).find('> div');
  		
  		title.each(function(e){
  		  var tabsEq = tabs.eq(e);
  		  title.eq(e)
  		    .click(function(){
  		      title.removeClass('active')
  		      $(this).addClass('active');
  		      tabs.hide();
  		      if(tabsEq.is(':hidden')){
  		        tabsEq.show()
  		      }else{
  		        tabsEq.hide()  		        
  		      }
  		    });
  		});
  	});
  };
  
  
  /** ********************************************************************
  
    * BEHAVIORS
    
    ** ***************************************************************** */
  
  
  Drupal.behaviors.angers = {
    attach: function (context, settings) {
      $('html').removeClass('no-js');
      
      // Function Enlarge your Clcik
      Drupal.angers.enlargeYourClick($('#block-angerslt-map-map, .view-mode-rubrique_enfant', context));
      
      // Function Primary Menu
      Drupal.angers.primarymenu($('.menu-block-1 > .menu > .expanded'), context);
      
      // Function Sidebar Tabs
      Drupal.angers.sidebarTabs($('#block-angerslt-search .tabs'), context);
      
      //Slide Rubriquage
      Drupal.angers.slideRubriquage($('.node-page-rubriquage > .item-list ul'), context);
      
      //Carousel prestataire
      Drupal.angers.carouselPrestataire($('.lcarrousel'), context);
      
      //Agenda Home
      //Drupal.angers.AgendaHome($('.tabs-container'), context);
      
      //Ubox SlideShow
      Drupal.angers.UboxSlideshow($('.ubox-slideshow'), context);
      
      //Slideshow OT
      Drupal.angers.SlideContactOT($('#block-alt-contact-alt-contact-contacts, #block-alt-contact-alt-contact-contacts-home'), context);

      //Slideshow Events
      Drupal.angers.SlideEvents($('#block-angerslt-agenda-slideshow-page'), context);

      //Tabs Events Home
      Drupal.angers.TabsEventsHome($('#block-angerslt-agenda-home'), context);

      //Tabs Events Home
      Drupal.angers.TabsFilterMap($('#angerslt-map-page-form'), context);

      //Tabs Comments and Vote
      Drupal.angers.TabsCommentVote($('.comments-and-vote'), context);
      
      if (Drupal.settings.geofieldMap) {
        // FOR IE8 and FF4 who don't support Object.keys
        Object.keys = Object.keys || (function () {
          var hasOwnProperty = Object.prototype.hasOwnProperty,
              hasDontEnumBug = !{toString:null}.propertyIsEnumerable("toString"),
              DontEnums = [
                  'toString',
                  'toLocaleString',
                  'valueOf',
                  'hasOwnProperty',
                  'isPrototypeOf',
                  'propertyIsEnumerable',
                  'constructor'
              ],
              DontEnumsLength = DontEnums.length;
       
          return function (o) {
              if (typeof o != "object" && typeof o != "function" || o === null)
                  throw new TypeError("Object.keys called on a non-object");
       
              var result = [];
              for (var name in o) {
                  if (hasOwnProperty.call(o, name))
                      result.push(name);
              }
       
              if (hasDontEnumBug) {
                  for (var i = 0; i < DontEnumsLength; i++) {
                      if (hasOwnProperty.call(o, DontEnums[i]))
                          result.push(DontEnums[i]);
                  }
              }
       
              return result;
          };
        })();
        // Get this map "id"
        var geofieldMapKey = Object.keys(Drupal.settings.geofieldMap).pop();
        // Remove map infoWindow description (= disable infoWindow)
        Drupal.settings.geofieldMap[geofieldMapKey].data.properties.description = "";
      }

    }
  };
  
}(jQuery));
;
