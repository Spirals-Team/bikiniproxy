  brand = "jeep";

  function lang_bt_click(){

    if(window.location.pathname == '/') {
        document.location.href = '/en/';
    }else if(window.location.pathname == '/ru/jeep-compass-2017/') {
        document.location.href = '/en/compass_2017/';
    } else{
        document.location.href = window.location.pathname.replace(/\/ru\//g,"/en/");
    }
  }

</script>

