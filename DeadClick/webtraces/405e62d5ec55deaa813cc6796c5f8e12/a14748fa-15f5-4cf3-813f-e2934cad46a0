function ninja_forms_post_add_tag(e){var t=jQuery(".tagchecklist > span").length;var n=1;jQuery(".tagchecklist > span").each(function(){if(this.id==e){n=0}});if(n==1){var r=e.replace(/[^A-Z0-9]+/ig,"_");console.log(r);var i='<span id="'+r+'" style="display:none;"><a id="post_tag-'+t+'" class="ninja-forms-del-tag">X</a>&nbsp;'+e+"</span>";jQuery(".tagchecklist").append(i);jQuery("#"+r).fadeIn();var s="";var o=0;jQuery(".tagchecklist > span").each(function(){if(o>0){s+=", "}s+=this.id;o++});jQuery("#ninja_forms_post_tag_hidden").val(s)}}jQuery(document).ready(function(e){e("#ninja_forms_post_add_tag").click(function(t){t.preventDefault();var n=e("#ninja_forms_post_tag").val();n=n.split(",");for(var r=n.length-1;r>=0;r--){var i=n[r].replace(/^\s\s*/,"").replace(/\s\s*$/,"");ninja_forms_post_add_tag(i)}e("#ninja_forms_post_tag").val("");e("#ninja_forms_post_tag").focus()});e("body").on("click",".ninja-forms-del-tag",function(t){t.preventDefault();e(this).parent().fadeOut("fast",function(){e(this).remove();var t="";var n=0;e(".tagchecklist > span").each(function(){if(n>0){t+=", "}t+=this.id;n++});e("#ninja_forms_post_tag_hidden").val(t)})});e(".ninja-forms-tag").click(function(e){e.preventDefault();var t=this.innerHTML;ninja_forms_post_add_tag(t)});e("#ninja_forms_show_tag_cloud").click(function(t){t.preventDefault();e("#ninja_forms_tag_cloud").fadeToggle()});e("body").on("click",".ninja-forms-terms-tab",function(t){t.preventDefault();e(this).parent().parent("> li").addClass("hide-if-no-js");e(this).parent().parent("> li").removeClass("tabs");e(this).parent().removeClass("hide-if-no-js");e(this).parent().addClass("tabs");var n=this.id.replace("_link","");e("."+this.name+"-tabs-panel").hide();e("#"+n).show()});e(".term-add-toggle").click(function(t){t.preventDefault();var n=this.id.replace("_add_toggle","");e("#"+n+"_add").toggle()});e(".new-term-label").focus(function(){var t=e(".new-term-label").val();if(this.value==t){this.value=""}});e(".new-term-label").blur(function(){var t=this.id.replace("_label","_default");var n=e("#"+t).val();if(this.value==""){this.value=n}});e(".term-add-submit").click(function(t){t.preventDefault();var n=this.id.replace("_add_submit","");var r=this.name.replace("new_","");r=r.replace("_tax","");e("#"+n+"_tabs > li").addClass("hide-if-no-js");e("#"+n+"_tabs > li").removeClass("tabs");e("#all_"+n+"_tab").removeClass("hide-if-no-js");e("#all_"+n+"_tab").addClass("tabs");e("."+n+"-tabs-panel").hide();e("#"+n+"_all").show();var i=true;var s=e("#new_"+n+"_label").val();var o=e(".new-"+n).length;o--;e("#"+n+"_checklist").find("span").each(function(){if(this.innerHTML.toLowerCase()==s.toLowerCase()){var t=e(this).prev(":checkbox:first").prop("id");e("."+t).prop("checked","checked");i=false;return false}});if(i){var u=e("#term_"+r+"_li_template").clone();var a=e("#"+n+"_parent").val();var f=e(u).find("label").prop("innerHTML");e(u).prop("id",n+"_new-"+o+"_li");e(u).attr("style","");e(u).find("span").prop("innerHTML",s);e(u).find(".term-parent").val(a);var l=e(u).find(".term-parent").prop("id");l=l.replace("[]","[new-"+o+"]");e(u).find(".term-parent").attr("name",l);e(u).find(":checkbox").val(s);var c=e(u).find(":checkbox").prop("id");c=c.replace("[]","[new-"+o+"]");e(u).find(":checkbox").attr("name",c);if(a==-1){e("#"+n+"_checklist").append(u);var h='<option value="new-'+o+'">'+s+"</option>";e("#"+n+"_parent").append(h)}else{if(e("#"+n+"_"+a+"_children").length==0){var h='<ul class="children termchecklist form-no-clear" id="'+n+"_"+a+'_children"></ul>';e("#"+n+"_"+a+"_li").append(h)}e("#"+n+"_"+a+"_children").append(u)}e("#"+n+"_parent").val(-1)}e("#new_"+n+"_label").val("").focus()});e("body").on("change",".cat-checkbox",function(){var t=this.checked;e("."+this.id).each(function(){this.checked=t})});e(".ninja-forms-feditor-delete-sub").click(function(e){return confirm(ninja_forms_feditor_settings.delete)});e(".ninja-forms-feditor-delete-post").click(function(e){return confirm(ninja_forms_feditor_settings.delete)})})