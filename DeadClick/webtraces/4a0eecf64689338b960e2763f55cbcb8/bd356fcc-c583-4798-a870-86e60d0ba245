function killLoader(){$(".tabcontent.tab-load").removeClass("loading").removeClass("tab-load");$(".tabcontent .container").css("display","block");}
function revealUploader(){if($(".add-content-expand").length>0){$(".add-content-expand").off('tap');$(".add-content-expand").on('tap',function(e){if($(".form-user-upload").hasClass("form-open")){$(".form-user-upload").removeClass("form-open").slideUp();}else{$(".form-user-upload").addClass("form-open").slideDown();}
e.preventDefault();});};};function post_load()
{killLoader();revealUploader();popUpGallery();hiddenGallery();selectBox();gridImages();videoPlayer();imageLoad();inlineGallery();refineHeight();refineChange();trackingDisplay();populateNocat();trackingHighlight();tables();toolTips();initPopups();fixSvgIconClicks()}
function initPopups()
{$("a.popup-btn").magnificPopup
({removalDelay:0,fixedContentPos:false,callbacks:{beforeOpen:function()
{this.st.mainClass=this.st.el.attr("data-effect");},parseAjax:function(mfpResponse)
{this.scripts=$(mfpResponse.data).find("script");mfpResponse.data=$.parseHTML(mfpResponse.data);},ajaxContentAdded:function()
{$(this.scripts).each
(function(index,value)
{$.globalEval(value.text);});}},midClick:true});$('.login-popup').magnificPopup
({type:'ajax',removalDelay:0,fixedContentPos:false,focus:'.select2-search__field',callbacks:{beforeOpen:function()
{this.st.mainClass=this.st.el.attr('data-effect');var body=$('body');var container=$('');var push=$('.push');var pushy=$('.pushy');body.toggleClass("pushy-active");pushy.toggleClass("pushy-left pushy-open");container.toggleClass("container-push");push.toggleClass("push-push");$("html, body").css("overflow-x","visible");},parseAjax:function(mfpResponse)
{this.scripts=$(mfpResponse.data).find("script");mfpResponse.data=$.parseHTML(mfpResponse.data);},ajaxContentAdded:function()
{$(this.scripts).each
(function(index,value)
{$.globalEval(value.text);});$('.mfp-content svg').each(function(){$(this).html($(this).html());});}},midClick:true});$('.popup-btn-ajax').magnificPopup
({type:'ajax',removalDelay:0,fixedContentPos:false,focus:'.select2-search__field',callbacks:{beforeOpen:function()
{this.st.mainClass=this.st.el.attr('data-effect');},parseAjax:function(mfpResponse)
{this.scripts=$(mfpResponse.data).find("script");mfpResponse.data=$.parseHTML(mfpResponse.data);},ajaxContentAdded:function()
{$(this.scripts).each
(function(index,value)
{$.globalEval(value.text);});$('.mfp-content svg').each(function(){$(this).html($(this).html());});}},midClick:true});}
function AjaxLoad(element_selector,destination,urlToPush)
{$(element_selector).load(destination);if(urlToPush)
{window.history.replaceState(null,null,urlToPush);}}
function PaginationJump(element,partial_url,js_target)
{var page=$(element).val();if(page>0)
{var url=partial_url+'page='+page;if(js_target==='')
{window.location=url;}
else
{AjaxLoad(js_target,url);}}}
function ApplySorting(url,js_target,unique_id)
{var src_order=$('#search_order_'+unique_id).val();var src_sort_by=$('#search_sortby_'+unique_id).val();var src_nresults=$('#search_nresults_'+unique_id).val();var src_timespan=$('#search_timespan_'+unique_id).val();var src_authortype=$('#search_authortype_'+unique_id).val();var src_game=$('#search_game_'+unique_id).val();if(typeof(src_order)!=='undefined')url+='&src_order='+src_order;if(typeof(src_sort_by)!=='undefined')url+='&src_sort_by='+src_sort_by;if(typeof(src_nresults)!=='undefined')url+='&src_nresults='+src_nresults;if(typeof(src_timespan)!=='undefined')url+='&src_timespan='+src_timespan;if(typeof(src_authortype)!=='undefined')url+='&src_author_type='+src_authortype;if(typeof(src_game)!=='undefined')url+='&src_game='+src_game;if(js_target==='')
{window.location=url;}
else
{AjaxLoad(js_target,url);}}
function isScrolledIntoView(el,top){var elemTop=el[0].getBoundingClientRect().top;var elemBottom=el[0].getBoundingClientRect().bottom;var isVisible=(elemTop>=0)&&(elemBottom<=window.innerHeight)&&($(window).scrollTop()<top);return isVisible;}
function LoadMoreModActionLogItems(mod_id,game_id,filter)
{blockUI({container:'.act-log-container',dimensions:'outer',static:true});var offs=actionLogOffset[filter];$.ajax({type:"POST",url:"/Core/Libs/Common/Widgets/ModActionLogMonthlyViewer",data:"game_id="+game_id+"&id="+mod_id+"&offset="+offs+'&filter='+filter,success:function(data)
{unblockUI('.act-log-container');$('#ModActionLogExpanderLastItem'+filter).before(data);}});}
function ToggleStickyTopic(topic_id,mod_id,game_id)
{$.ajax({type:"POST",url:"/Core/Libs/Common/Entities/ModComment?ToggleStickyTopic",data:"game_id="+game_id+"&mod_id="+mod_id+"&topic_id="+topic_id,success:function(msg)
{if(msg!=1&&msg!=0)
{alert(msg);}
else
{AjaxLoad('.tabcontent','/Core/Libs/Common/Widgets/ModTopicsTab?id='+mod_id+'&game_id='+game_id);}}});return false;}
function DeleteTopic(topic_id,mod_id,game_id)
{if(confirm("Are you sure you want to delete this topic?")!==true)
{return false;}
$.ajax({type:"POST",url:"/Core/Libs/Common/Entities/ModComment?DeleteTopic",data:"game_id="+game_id+"&mod_id="+mod_id+"&topic_id="+topic_id,success:function(msg)
{if(msg!=1)
{alert(msg);}
else
{AjaxLoad('.tabcontent','/Core/Libs/Common/Widgets/ModTopicsTab?id='+mod_id+'&game_id='+game_id);}}});return false;}
function AddTopic(mod_id,game_id)
{var post=$('#add-topic-post').val();var description=$('#add-topic-description').val();var subject=$('#add-topic-subject').val();var stopIt=false;var errors='';if(post.trim()==='')
{$('#add-topic-post').addClass("error-input");errors+='<p>Please enter some text</p>';stopIt=true;}
else
{$('#add-topic-post').removeClass('error-input');}
if(subject.trim()==='')
{$('#add-topic-subject').addClass("error-input");errors+='<p>Please specify the subject</p>';stopIt=true;}
else
{$('#add-topic-subject').removeClass('error-input');}
if(stopIt===true)
{$('#add-topic-error-message').html(errors);$('#add-topic-error-box').show();return false;}
else
{$('#add-topic-error-box').hide();}
$.ajax({type:"POST",url:"/Core/Libs/Common/Entities/ModComment?AddTopic",data:"game_id="+game_id
+"&mod_id="+mod_id
+"&subject="+subject
+"&description="+description
+"&post="+post,success:function(msg)
{if(msg!=1)
{$('#add-topic-error-message').html('<p>'+msg+'</p>');$('#add-topic-error-box').show();}
else
{$('.mfp-close').click();AjaxLoad('.tabcontent','/Core/Libs/Common/Widgets/ModTopicsTab?id='+mod_id+'&game_id='+game_id);}}});return false;}
function endorseImage(image_id,game_id,is_supporter)
{$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Images?EndorseImage",data:"image_id="+image_id+"&game_id="+game_id+'&is_supporter='+(is_supporter?'1':'0'),success:function(res)
{var errors=res['errors'];var endorsements=res['endorsements'];var is_endorsed=res['is_endorsed'];if(errors!=='')
{alert(errors);}
else
{if(is_endorsed===1)
{$('#button-endorse').addClass('btn-active');$('#action-endorse').attr('title','Click to remove your endorsement');$('#text-endorse').html('Endorsed');}
else
{$('#button-endorse').removeClass('btn-active');$('#action-endorse').attr('title','Click to endorse this image');$('#text-endorse').html('Endorse');}
$('#endorsement-number').html(endorsements);}}});return false;}
function endorseVideo(video_id,game_id)
{$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Videos?EndorseVideo",data:"video_id="+video_id+"&game_id="+game_id,success:function(res)
{var errors=res['errors'];var endorsements=res['endorsements'];var is_endorsed=res['is_endorsed'];if(errors!=='')
{alert(errors);}
else
{if(is_endorsed===1)
{$('#button-endorse').addClass('btn-active');$('#action-endorse').attr('title','Click to remove your endorsement');$('#text-endorse').html('Endorsed');}
else
{$('#button-endorse').removeClass('btn-active');$('#action-endorse').attr('title','Click to endorse this video');$('#text-endorse').html('Endorse');}
$('#endorsement-number').html(endorsements);}}});return false;}
function addNewComment(thread_id,game_id,object_id,object_type,parent_id)
{var post=encodeURIComponent($('#add-comment-post-'+parent_id).val());var use_emo=$('#add-comment-use-emoticons-'+parent_id).is(":checked")?1:0;if(post.trim()==='')
{$('#add-comment-error-message-'+parent_id).html('Please enter some text');$('#add-comment-error-box-'+parent_id).show();return;}
$('#add-comment-error-message-'+parent_id).html('');$('#add-comment-error-box-'+parent_id).hide();$('#submit-comment-'+parent_id).after(smallLoadingWheel('wheel-comment-'+parent_id));$('#submit-comment-'+parent_id).hide();$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Forum?AddNewComment",data:"game_id="+game_id
+"&object_id="+object_id
+"&object_type="+object_type
+"&thread_id="+thread_id
+"&post="+encodeURIComponent(post)
+"&use_emo="+use_emo
+"&parent_id="+parent_id,success:function(msg)
{if(msg!=1)
{$('#wheel-comment-'+parent_id).hide();$('#submit-comment-'+parent_id).show();$('#add-comment-error-message-'+parent_id).html(msg);$('#add-comment-error-box-'+parent_id).show();}
else
{reloadTab();}}});}
function smallLoadingWheel(id)
{return'<div class="small-loading-wheel" id="'+id+'"></div>';}
function loadingWheel(id)
{return'<div class="loading-wheel" style="display:block;" id="'+id+'"></div>';}
function editComment(comment_id)
{var errors='';var content='';var post=encodeURIComponent($('#edit-comment-post-'+comment_id).val());var use_emo=$('#edit-comment-use-emoticons-'+comment_id).is(":checked")?1:0;if(post.trim()==='')
{$('#edit-comment-error-message-'+comment_id).html('Please enter some text');$('#edit-comment-error-box-'+comment_id).show();return;}
$('#edit-comment-error-message-'+comment_id).html('');$('#edit-comment-error-box-'+comment_id).hide();$('#submit-edit-comment-'+comment_id).after(smallLoadingWheel('wheel-'+comment_id));$('#submit-edit-comment-'+comment_id).hide();$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Forum?EditComment",data:"comment_id="+comment_id
+"&post="+encodeURIComponent(post)
+"&use_emo="+use_emo,success:function(res)
{$('#wheel-'+comment_id).hide();$('#submit-edit-comment-'+comment_id).show();if(typeof(res)==='object')
{errors=res['errors'];content=res['content'];}
else
{errors=res;}
if(errors!='')
{$('#edit-comment-error-message-'+comment_id).html(errors);$('#edit-comment-error-box-'+comment_id).show();}
else
{$('#edit-comment-form-'+comment_id).hide();$('#comment-content-'+comment_id).html(content);$('#comment-content-'+comment_id).show();reloadTab();}}});}
function lock_comment(comment_id,object_id,object_type,game_id,is_lock)
{$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Forum?ToggleLockComment",data:"comment_id="+comment_id
+"&object_id="+object_id
+"&object_type="+object_type
+"&game_id="+game_id
+"&is_lock="+is_lock,success:function(res)
{var errors=res['errors'];var content=res['content'];if(errors!=='')
{alert(errors);}
else
{if(content===1)
{$('#locked-comment-label-'+comment_id).show();$('#lock-comment-'+comment_id).hide();$('#unlock-comment-'+comment_id).show();}
else
{$('#locked-comment-label-'+comment_id).hide();$('#lock-comment-'+comment_id).show();$('#unlock-comment-'+comment_id).hide();}}}});return false;}
function stick_comment(comment_id,object_id,object_type,game_id,is_sticky,move)
{DisplayWaitingPopup();$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Forum?ToggleStickyComment",data:"comment_id="+comment_id
+"&object_id="+object_id
+"&object_type="+object_type
+"&game_id="+game_id
+"&move="+move
+"&is_sticky="+is_sticky,success:function(res)
{if(res!==1)
{alert(res);}
else
{reloadTab();}
ClosePopUp();}});return false;}
function reloadTab()
{var js_target=$("#reload-js-target").val();var url=$("#reload-tab-url").val();AjaxLoad(js_target,url);}
