else
{DisplayPopup("There was a problem with your request",msg['message'].toString());}});},debug:true,invalidHandler:function(){},rules:{video_terms:{required:true},video_url:{url:true,required:true,minlength:30,maxlength:100}},messages:{video_terms:{required:"You must agree to our terms and conditions for displaying this video"},video_url:{url:"This must be a valid URL",required:"You must provide a valid YouTube Link",minlength:"That URL doesn't seem to be long enough"}},ignore:[],errorPlacement:function(error,element){if(element.attr("name")=="video_terms"){error.insertAfter("#video_terms_list");}else{error.insertAfter(element);}
