function changeBugIssueStatus(issue_id,status)
{var selectedValue=status;$.ajax({type:"POST",url:"/Bugs?ChangeBugStatus",data:"issue_id="+issue_id+"&status="+selectedValue,success:function(msg)
{if(selectedValue==0)
{$("#issueStatus_"+issue_id+" span").html('New issue').removeClass().addClass('orange');}
else if(selectedValue==2)
{$("#issueStatus_"+issue_id+" span").html('Being looked at').removeClass().addClass('orange');}
else if(selectedValue==3)
{$("#issueStatus_"+issue_id+" span").html('Fixed').removeClass().addClass('green');}
else if(selectedValue==1)
{$("#issueStatus_"+issue_id+" span").html('Known issue').removeClass().addClass('orange');}
else if(selectedValue==4)
{$("#issueStatus_"+issue_id+" span").html('Duplicate').removeClass();}
else if(selectedValue==5)
{$("#issueStatus_"+issue_id+" span").html('Not a bug').removeClass().addClass('green');}
else if(selectedValue==6)
{$("#issueStatus_"+issue_id+" span").html('Won\'t fix').removeClass().addClass('red');}
else if(selectedValue==7)
{$("#issueStatus_"+issue_id+" span").html('Needs more info').removeClass().addClass('orange');}
$("#changeissuestatus_"+issue_id).toggle();if(selectedValue==3)$("#issue_fixed_container_"+issue_id).show();else $("#issue_fixed_container_"+issue_id).hide();}});}
