function loadNotifications(iwantmore)
{if(did_I_open_my_notifications==false||iwantmore==true)
{did_I_open_my_notifications=true;$.ajax({type:"POST",url:"/Notifications?GetNotificationTiles",data:"offset="+currentNotifOffset+'&game_id='+notifications_game_id,success:function(msg)
{if(msg['status']===true)
{setNoNewNotifications();if(currentNotifOffset==0)
{$('.notification-loading').remove();$(".load-more-updates").show();}
