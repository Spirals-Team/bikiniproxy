{$('#'+file.uniqueIdentifier).find('.progress').html("There has been an error with this file");});};function mod_quicksearch(term,game_id,blocked_tags,blocked_authors,include_adult){include_adult=typeof include_adult!=='undefined'?include_adult:false;$.get("https://api.nexusmods.com/v1/quicksearch/mods",{terms:term,game_id:game_id,blocked_tags:blocked_tags,blocked_authors:blocked_authors,include_adult:include_adult}).done(function(data){parse_results(data);}).fail(function(){console.log("QuickSearch Error");});}
function parse_results(data){$("#gsearch_results").html("");var search_terms=data.terms;var blocked_authors=data.exclude_authors;var blocked_tags=data.exclude_tags;var results=data.results;var ms_taken=data.took;var total_results=data.total;var rendered_results=[];var result_count=1;$.each(results,function(key,value){rendered_results.push(" <li class='qs_result_li' id='qs_result_"+result_count+"' data-id='"+result_count+"'>             <a href='"+value.url+"'>                 <div class='qs_result'>                     <section>                         <figure class='image_figure'>                             <img class='back' src='/Contents/images/noimage.svg' width='600' height='338'>                             <div class='fore_div_mods'>                                 <img class='fore' onerror='imgError(this);' src='https://staticdelivery.nexusmods.com"+value.image+"'/>                             </div>                         </figure>                         <div>                            <h2>"+value.game_name+"</h2>                             <h3>"+value.name+"</h3>                             <div class='stats'>                                 <span class='author'> <svg class='icon icon-friend'><use xlink:href='/contents/images/icons.svg#icon-friend'></use></svg> <span class='qs_text'>"+value.username+"</span></span>                                 <span class='downloads'> <svg class='icon icon-downloads'><use xlink:href='/contents/images/icons.svg#icon-downloads'></use></svg> <span class='qs_text'>"+value.downloads.toLocaleString("en")+"</span></span>                                <span class='endorsements'> <svg class='icon icon-endorse'><use xlink:href='/contents/images/icons.svg#icon-endorse'></use></svg> <span class='qs_text'>"+value.endorsements.toLocaleString("en")+"</span></span>                             </div>                         </div>                     </section>                </div>             </a>         </li>");result_count++;});if(results.length<1){$("#gsearch_results").append("<div id='qs_results'><span>Quicksearch returned 0 results in "+ms_taken+"ms.</span></div>");}else{$("#gsearch_results").append("<div id='qs_results'><span>Quicksearch returned "+results.length+" of <a onclick=\"$('#main-search').click();\" title=\"See all results\">&asymp;"+total_results+" results</a> in "+ms_taken+"ms.</span></div>");$("#qs_results").append("<div id='qs_pagination'></div>");$("#gsearch_results").append("<ul class='gsearch_results_list' id='qsearch_results'></ul>");$.each(rendered_results,function(key,value){$("#qsearch_results").append(value);});}
$("#qs_pagination").append("<a class='qs_close' id='qs_close' onclick='$(\"#gsearch_results\").hide();' href='#'>x</a>");$("#gsearch_results").show();pagination_keys();$("#gsearch").blur();select_result(1);$(".qs_result_li").hover(function(){select_result($(this).data("id"))});}
$(document).ready(function(){window.last_quick_search="";var thread=null;$('#gsearch').keyup(function(){if($("#searchtype option:selected").val()=="mods"){var term=$.trim($("#gsearch").val());if(window.last_quick_search!=term){clearTimeout(thread);var $this=$(this);thread=setTimeout(function(){if(term.length>1){term=term.replace("-"," ");var search_string=term.split(" ").join();var game_id=$(document).data('nexus.game_id');var blocked_tags=$(document).data('nexus.blocked_tags');var blocked_authors=$(document).data('nexus.blocked_authors');var include_adult=$(document).data('nexus.include_adult');mod_quicksearch(search_string,game_id,blocked_tags,blocked_authors,include_adult);window.last_quick_search=term;}},300);}}});$("#searchtype").change(function(){if($("#searchtype option:selected").val()=="mods"){$('#gsearch').keyup();}else{$("#gsearch_results").hide();}});});function select_result(id,autoscroll){autoscroll=typeof autoscroll!=='undefined'?autoscroll:false;$('.qs_result_li').removeClass("active");$("#qs_result_"+id).addClass("active");if(autoscroll){start_offset=$('#qs_pagination').offset().top+57;offset=((parseInt($('.qs_result_li.active').data().id)-1)*85)-start_offset;$('#qsearch_results').scrollTop(offset);}}
function pagination_keys(){$(document).off("keydown");$(document).keydown(function(e){current_result=parseInt($('.qs_result_li.active').data("id"));last_result=parseInt($('.qs_result_li').last().data("id"));switch(e.which){case 9:$('#gsearch').focus();$(document).off("keydown");break;case 27:$('#gsearch').focus();$(document).off("keydown");$("#gsearch_results").hide();break;case 37:if(current_result<6){select_result(1,true);}else{select_result((current_result-6),true);}
break;case 38:if(current_result>1){select_result(current_result-1,true);}
break;case 39:if((current_result+6)<last_result){select_result(current_result+6,true);}else{select_result(last_result,true);}
break;case 40:if(current_result<last_result){select_result(current_result+1,true);}
break;default:$('#gsearch').focus();$(document).off("keydown");return;}
e.preventDefault();});}
jQuery(document).ready(function($){var offset=300,offset_opacity=1200,scroll_top_duration=700,$back_to_top=$('.to-top');$(window).scroll(function(){($(this).scrollTop()>offset)?$back_to_top.addClass('top-is-visible'):$back_to_top.removeClass('top-is-visible top-fade-out');if($(this).scrollTop()>offset_opacity){$back_to_top.addClass('top-fade-out');}});$back_to_top.on('click',function(event){event.preventDefault();$('body,html').animate({scrollTop:0,},scroll_top_duration);});});function Accordion()
{this.GlobalLoad();}
