{this.GlobalLoad();this.RH=args.RequestHelper;this.attributes=args.attributes;$("#"+this.RH.group_id+"refine_loading").hide();function ParseScale(n,min,max,pretty)
{if(n<1)
{return 0;}
var min_log=(min>0)?Math.log(min):Math.log(1);var max_log=Math.log(max);var scale=(max_log-min_log)/(max-min);var res=Math.exp(min_log+scale*(n-min));if(pretty===undefined)
{return res.toFixed(0);}
else
{return res.toFixed(0).toLocaleString();}};function ValueToScale(n,min,max)
{if(n<1)
{return 0;}
