else
{DisplayPopup("Error",msg['message']);}}});return false;}};$('body').on('mousedown','.notification-link',function(event){if(event.which===2){openAndMarkAsRead(event,$(this));}});$('body').on('click','.notification-link',function(event){openAndMarkAsRead(event,$(this));});$('body').on('click','.remove-notification',function(event)
