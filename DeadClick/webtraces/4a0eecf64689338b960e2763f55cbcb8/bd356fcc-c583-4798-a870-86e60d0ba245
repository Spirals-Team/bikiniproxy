{$('#game-picker-widget').replaceWith(data);$('#filter-games').val(name);$('#game-suggestion-container').hide();$("#filter-games").change();$('#game_id').val(game_id);$('#game-tile-'+game_id).click();FilterGames('',formName,game_id);}});}
else
{DisplayPopup('Error',msg['message']);}}});}
return false;});});function FilterGames(element,formId,game_id)
{if(game_id==0)
{$("#filter-games").val($(element).find('h3').text());game_id=$(element).parents('li').attr('id').replace('game-tile-','');}
