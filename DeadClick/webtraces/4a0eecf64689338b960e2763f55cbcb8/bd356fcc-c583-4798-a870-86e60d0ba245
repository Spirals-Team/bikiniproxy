{$(".no-more-updates").show();$(".load-more-updates").hide();}
else
{$(".remove-updates").show();}
$('.notification-link').off('click').on('click',function(e){var notification=$(e.target);if(notification.data('type')==='comment'){if($('#comment-'+notification.data('id')).length){e.preventDefault();e.stopPropagation();$('html, body').animate({scrollTop:$('#comment-'+notification.data('id')).offset().top},200);return false;}}});}
else
{DisplayPopup("Error",msg['message']);}}});}}
function setNoNewNotifications()
{$.ajax({type:"POST",url:"/Notifications?SetNoNewNotifications",data:'game_id='+notifications_game_id,success:function(msg)
{unreadNotifications=0;iconStatus=0;updateNotificationNumber();}});}
var notificationPoll=function()
{$.ajax({type:"POST",url:"/Notifications?RefreshStatus",data:'game_id='+notifications_game_id,success:function(msg)
{if(msg['status']!=true)
{console.log(msg['message']);}
