{if(msg['status'])
{$("#"+game_id+"_"+image_id+" .mod-tile-left").addClass('verified_image');$("#"+game_id+"_"+image_id).fadeTo("slow",0.5,function()
{$("#"+game_id+"_"+image_id).detach().appendTo('#user_approved_images_list')
hide_emtpy_mod_image_section();});$("#"+game_id+"_"+image_id+" .validate_image").hide();}
else
{DisplayPopup("There was a problem with your request",msg['message'].toString());}});}
