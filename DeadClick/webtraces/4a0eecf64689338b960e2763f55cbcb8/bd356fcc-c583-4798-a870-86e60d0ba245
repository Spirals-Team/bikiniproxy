if($('#game_suggested_categories').length)
{$.ajax({type:"GET",url:"/GameCategories?GetSuggestedCategories",data:"game_id="+game_id,dataType:"html",success:function(msg){$('#game_suggested_categories').html(msg);$('#game_suggested_categories').find('option[value=1]').remove();}});}
