else
{DisplayPopup('Error',msg['message']);}}});});});var scrollInterval=null;function blockUI(args)
{var div,width,height,dimensions,top,el,static=false;if(args instanceof jQuery)
{div=args;}
else if(typeof args==='object')
{div=args.container instanceof jQuery?args.container:$(args.container);dimensions=args.dimensions?args.dimensions:'inner';static=args.static?args.static:false;}
else
{div=$(args);}
if(dimensions==='outer')
{width=div.outerWidth();height=div.outerHeight();}
else
{width=div.width();height=div.height();}
var blocker=$('<div class="nexus-ui-blocker"></div>');var msgbox=$('<div class="nexus-ui-blocker-message-box"></div>');var loader=$('<div class="flex"><div class="col-1-2 loading-wheel" style="display:block; margin-top:24px; margin-bottom:24px;"></div></div>');msgbox.append(loader);div.css({position:'relative'});blocker.css({width:width+'px',height:height+'px'});blocker.append(msgbox);div.append(blocker);el=$(".nexus-ui-blocker-message-box");if(el.length)
{top=el.offset().top;if(!static)
{var windowOffs=$(window).scrollTop();var windowHeight=$(window).height();scrollInterval=setInterval(function()
{if((windowOffs+windowHeight-100)<top)
{el.addClass('fixed');}
else
{el.removeClass('fixed');}},1);}}}
function unblockUI(container){$(container).find('.nexus-ui-blocker').remove();clearInterval(scrollInterval);scrollInterval=null;}
function DisplayPopup(title,content,extraClasses,onCloseFunction)
{var popUpContent='<div class="popup-js popup mfp-with-anim col-1-1">'+'<div class="col-1-1 flex clearfix">'+'<div class="col-1-1">'+'<h2>'+title+'</h2>'+'<p class="info inline-flex">'+'<span class="flex-copy">'+
content+'</span>'+'</p>'+'</div>'+'</div>'+'</div>';$.magnificPopup.open({mainClass:extraClasses,fixedContentPos:false,items:{src:popUpContent,type:'inline'},callbacks:{close:function()
{if(onCloseFunction)
{eval(onCloseFunction);}}}});}
function ClosePopUp()
{$('.mfp-close').click();}
function DisplayWaitingPopup()
{var popUpContent='<div class="popup-login-register popup mfp-with-anim" style="width:100px;">'+'<div class="flex">'+'<div class="col-1-2 loading-wheel" style="display:block; margin-top:24px; margin-bottom:24px;"></div>'+'</div>'+'</div>';$.magnificPopup.open({items:{src:popUpContent,type:'inline'},fixedContentPos:false});}
