$(function()
{$('body').on('click','#submit-object-report',function(event)
{var object_id=$(this).data('objectId');var object_type=$(this).data('objectType');var game_id=$(this).data('gameId');var category=$('#problem-category-dropdown').val();var description=$('#problem-description').val();if(category.trim()===''||category==='0')
{$('#report-error-message').html('<p>Please select a category</p>');$('#report-error-box').show();}
else if(description.trim()==='')
{$('#report-error-message').html('<p>Please enter a description</p>');$('#report-error-box').show();}
else
{$('#report-error-box').hide();$('#submit-object-report').after(smallLoadingWheel('wheel-object-report'));$('#submit-object-report').hide();$.ajax({type:"POST",url:"/Core/Libs/Common/Managers/Reports?AddReport",data:"object_id="+object_id
+"&object_type="+object_type
+"&game_id="+game_id
+"&category="+category
+"&url="+window.location.href
+"&size="+window.adSize
